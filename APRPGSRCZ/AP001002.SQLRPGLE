     C                   EVAL      ZADIG = 1
     C                   EVAL      ZADEC = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZICALC2
     C     RCAPGM        WHENEQ    '14'
     C                   EVAL      ZADIG = 15
     C                   EVAL      ZADEC = 0
     C                   EVAL      ChkPos = %SCAN('.':RDETAIL)
     C                   IF        ChkPos = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   ELSE
     C                   EVAL      ZFIELD = %SUBST(RDETAIL:1:ChkPos-1)
     C                   ENDIF
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIAMT2
     C     RCAPGM        WHENEQ    '15'
