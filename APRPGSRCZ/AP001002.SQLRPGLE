     C                   EVAL      ZADIG = 5
     C                   EVAL      ZADEC = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIEND
     C     RCAPGM        WHENEQ    '3'
     C                   EVAL      ZADIG = 1
     C                   EVAL      ZADEC = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZICALC
     C     RCAPGM        WHENEQ    '4'
     C                   EVAL      ZADIG = 15
     C                   EVAL      ZADEC = 0
     C                   EVAL      ChkPos = %SCAN('.':RDETAIL)
     C                   IF        ChkPos = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   ELSE
     C                   EVAL      ZFIELD = %SUBST(RDETAIL:1:ChkPos-1)
     C                   ENDIF
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIAMT
     C     RCAPGM        WHENEQ    '5'
     C                   EVAL      ZADIG = 4
     C                   EVAL      ZADEC = 7
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIRATE
     C     RCAPGM        WHENEQ    '11'
     C                   EVAL      ZADIG = 5
     C                   EVAL      ZADEC = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIBEG2
     C     RCAPGM        WHENEQ    '12'
     C                   EVAL      ZADIG = 5
     C                   EVAL      ZADEC = 0
     C                   MOVEL     RDETAIL       ZFIELD
     C                   EXSR      ZALIGN
     C                   MOVE      ZFIELD        WZIEND2
