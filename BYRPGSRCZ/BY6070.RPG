     C                     MOVE 'D'       I#MDIN
     C                     ENDIF
      *
     C                     MOVEL'GBP'     I#TCCY
     C                     Z-ADD0         I#TAMT
      *
     C                     CALL 'AOCCYCV0'
     C                     PARM *BLANKS   ULRTCD
     C                     PARM '*F*T*RAT'W0ACT   8
     C           I#DTA     PARM I#DTA     P0DTA 256
      *
     C           ULRTCD    IFNE *BLANK
     C           *LOCK     IN   LDA
     C                     MOVELULPGMN    DBPGM            ************
