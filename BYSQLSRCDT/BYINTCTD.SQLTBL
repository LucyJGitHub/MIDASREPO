   LAMODE IS 'Mode'                                                             :
  ) ;                                                                           :
                                                                                :
  LABEL ON COLUMN **********/**********                                         :
  (                                                                             :
   LAINSC TEXT IS 'Institution Code' ,                                          :
   LAINSN TEXT IS 'Institution name' ,                                          :
   LAUKRI TEXT IS 'UK Residency Indicator (Y/N)' ,                              :
   LAUKMI TEXT IS 'Monetary Sector Indicator (Y/N)' ,                           :
