  , LAUKMI CHAR(1) NOT NULL DEFAULT ''                                          :
  , LABINC CHAR(5) NOT NULL DEFAULT ''                                          :
  , LAMODE CHAR(1) NOT NULL DEFAULT ''                                          :
  )                                                                             :
                                                                                :
