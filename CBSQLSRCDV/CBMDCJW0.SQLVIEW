  )                                                                             :
  as select                                                                     :
       x.DJCOTT                                                                 :
     , x.DJCSEQ                                                                 :
     , x.DJMCUN                                                                 :
     , u.DJMD01                                                                 :
     , u.DJMP01                                                                 :
