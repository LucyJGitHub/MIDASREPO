     , u.DJMD10                                                                 :
     , u.DJMP10                                                                 :
     , x.DJMODE                                                                 :
  from CBMDCUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  CBMDCXTD x                                                                    :
  on                                                                            :
      u.DJCOTT = x.DJCOTT and u.DJCSEQ = x.DJCSEQ and                           :
      u.DJMCUN = x.DJMCUN                                                       :
                                                                                :
  rcdfmt CBMDCJT0;                                                              :
