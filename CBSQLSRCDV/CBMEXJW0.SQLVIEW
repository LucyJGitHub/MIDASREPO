                                                                                :
  create view                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       DKCOTT                                                                   :
     , DKCSEQ                                                                   :
     , DKMEUN                                                                   :
     , DKMECN                                                                   :
     , DKMECS                                                                   :
     , DKMODE                                                                   :
  )                                                                             :
  as select                                                                     :
       x.DKCOTT                                                                 :
     , x.DKCSEQ                                                                 :
     , x.DKMEUN                                                                 :
     , x.DKMECN                                                                 :
     , x.DKMECS                                                                 :
     , x.DKMODE                                                                 :
  from CBMEXUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  CBMEXXTD x                                                                    :
  on                                                                            :
      u.DKCOTT = x.DKCOTT and u.DKCSEQ = x.DKCSEQ and                           :
      u.DKMEUN = x.DKMEUN                                                       :
                                                                                :
  rcdfmt CBMEXJT0;                                                              :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas CB Mutually Exclusive Compnt Joint        ';                       :
