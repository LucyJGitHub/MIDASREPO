     C*  Calls       :  None                                          *
     C*                                                               *
     C*****************************************************************
     C*
     C           SRRFMN    BEGSR                           ** SRRFMN **
     C*
     C**  Set up subroutine stack name.
     C*
     C                     ADD  1         Q
     C                     MOVEL'SRRFMN'  @STK,Q
     C*
     C                     Z-ADD0         #1
     C*
     C**  Loop through RDE's and data.
     C*
     C           #1        DOUEQ20
     C*
     C                     ADD  1         #1
     C                     MOVEL##R,#1    R#DEFN
     C                     MOVEL##D,#1    R#DATA
     C*
     C**  If data present and RDE is edited.
     C*
     C           R#DATA    IFNE *BLANK
     C           ##RDEC    ANDNE*BLANK
     C*
     C**  Reformat Amount.
     C*
     C                     Z-ADD1         #2
     C           *BLANK    LOKUP##NUMA,#2                99*
     C                     Z-ADD20        #3
     C                     Z-ADD0         ##WNUM
     C*
     C           #2        DOWGT1
     C           #2        ANDLE20
     C           #3        ANDGT1
     C                     SUB  1         #2
     C                     MOVEL##NUMA,#2 ##W,#3
     C                     SUB  1         #3
     C                     ENDDO
     C*
     C**  Sign the amount.
     C*
     C           ##SIGN    IFEQ '-'
     C                     Z-SUB##WNUM    ##NUMB
     C                     ELSE
     C                     Z-ADD##WNUM    ##NUMB
     C                     END
     C*
     C**  Default Edit Type.
     C*
     C           ##EDTT    IFEQ *BLANK
     C                     MOVEL##RDET    ##EDTT
     C                     END
     C*
     C**  Default Decimal Places.
     C*
     C           ##DCPA    IFEQ *BLANK
     C                     MOVEL##RDED    ##DCPA
     C                     END
     C*
     C**  New RDE data.
     C*
     C                     MOVELR#DATA    ##D,#1
     C*
     C                     ENDIF
     C*
     C                     ENDDO
     C*
     C           EXRFMN    TAG
