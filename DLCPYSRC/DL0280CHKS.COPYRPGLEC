     C     AKY(3)        OREQ      'M'
     C     DTYP          IFEQ      'IP'
     C     FSRMAC        ANDEQ     'Y'
     C     DTYP          OREQ      'TI'
     C     FSRMAC        ANDEQ     'Y'
     C                   MOVE      'Y'           MULTIF
     C                   MOVE      'R'           RPIND
     C                   ENDIF
