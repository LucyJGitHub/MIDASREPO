0491 C                     Z-ADDZZTOTD    ZZSTRD  30                      DL0265
0492 C*    STEP #2                                                        DL0265
0493 C                     Z-SUBZZINT     ZZAMTI 150                      DL0265
0494 C                     Z-SUBZZDEC     ZZAMTD  30                      DL0265
0495 C*    STEP #3                                                        DL0265
0496 C                     EXSR GLZSUM                                    DL0265
0497 C   99                GOTO ZZQUIT                                    DL0265
0498 C*    STEP #4                                                        DL0265
0499 C                     Z-ADDZZTOTI    ZZDIFI                          DL0265
0500 C                     MOVE ZZTOTD    ZZDIFD                          DL0265
0501 C*    STEP #5                                                        DL0265
0502 C                     Z-ADDZZSTRI    ZZTOTI 150                      DL0265
0503 C                     Z-ADDZZSTRD    ZZTOTD  30                      DL0265
0504 C                     GOTO ZZCEND                     BYPASS ADMIT   DL0265
