     C           CIR005    IFEQ 'Y'                                       CIR005
     C           WIPFR     ANDNE'Z'                                       CIR005
     C           WAMCN     ANDEQ'A'                                       CIR005
     C           WNIPD     ANDLEWNICD                                     CIR005
     C           WIPBD     ANDNE0                                         CIR005
     C           WRTFL     ANDNE'Y'                                       CIR005
     C                     Z-ADDWIPBD     WICED                           CIR005
     C                     ENDIF                                          CIR005
     C*
     C                     ENDSR
     C*
