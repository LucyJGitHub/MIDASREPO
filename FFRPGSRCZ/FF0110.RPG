     C                     EXSR ASCHGS                                    S01179
     C*                                                                   S01179
     C** CK3 - TERMINATE
     C*
     C           AID       IFEQ '01'
     C                     MOVEA'1'       *IN,12
     C                     END
     C*
     C** CK12- INITIAL SCREEN
     C*
     C           AID       IFEQ '05'
     C                     MOVEA'1'       *IN,10
     C                     END
     C*
     C** HELP - CALL HELP PROGRAM
     C*
     C********** AID       IFEQ 'HP'                                      S01199
     C**********           MOVELHERCD     ERRMSG                          S01199
     C********** ERR,1     IFNE *BLANK                                    S01199
     C**********           MOVELHI        ERRMSG                          S01199
     C**********           END                                            S01199
     C**********           CALL 'MHELP'                                   S01199
     C**********           PARM           HBPGM                           S01199
     C**********           PARM           HERCD                           S01199
     C**********           END                                            S01199
     C*
     C** PAGE FORWARD TAKEN, READ NEXT RECORD
     C*
     C           AID       IFEQ 'UP'
     C           ACTN      ANDNE'I'
     C                     MOVELACTNX     ACTN                            E15718
     C                     READ MARKTX                   12
     C*
     C** CHANGE MESSAGE IF END OF FILE REACHED - NO ROLL FWD
     C*
     C           *IN,12    IFEQ '1'
     C                     MOVELINF,8     INFMSG
     C                     ELSE
     C                     MOVEA'1'       *IN,12
     C                     END
     C                     END
     C*
     C** PAGE BACK TAKEN, READ PREVIOUS RECORD
     C*
     C           AID       IFEQ 'DN'
     C           ACTN      ANDNE'I'
     C                     MOVELACTNX     ACTN                            E15718
     C                     READPMARKTX                   12
     C*
     C** CHANGE MESSAGE IF BEGINNING OF FILE REACHED - NO ROLL BACK
     C*
     C           *IN,12    IFEQ '1'
     C                     MOVELINF,9     INFMSG
     C                     ELSE
     C                     MOVEA'1'       *IN,12
     C                     END
     C                     END
     C*
     C** NOT CK3,CK12,HELP,ROLLUP,ROLLDOWN
     C*
     C           AID       IFEQ 'RA'
     C           AID       OREQ '04'
     C*
     C** V A L I D A T E   D E T A I L   S C R E E N   I N P U T
     C*
     C                     EXSR VALIDD
     C*
     C** N O    D E T A I L   S C R E E N    E R R O R S   -  S T A R T
     C*
     C           SCRMSG    IFEQ *BLANK
     C           AID       ANDEQ'RA'
      *                                                                                       CER001
     C                     MOVELACTN      ACTION                                              CER001
     C                     MOVELMRKT      #1MRKT                                              CER001
     C           ULX008    IFEQ 'Y'                                                           CER001
     C                     CALL 'FF0111'                                                      CER001
     C                     PARM *BLANK    RTCODE  7                                           CER001
     C                     PARM           ACTION  1                                           CER001
     C**********           PARM           #1MRKT  2                                  CER001 BUG10372
     C                     PARM           WDATA                                             BUG10372
     C                     PARM *BLANK    W0RTN   7                                           CER001
      *                                                                                       CER001
      ** If F12 OR F3 is pressed                                                              CER001
      *                                                                                       CER001
     C           W0RTN     IFNE *BLANK                                                        CER001
     C           W0RTN     IFNE 'USR0790'                                                     CER001
     C           W0RTN     ANDNE'Y2U9999'                                                     CER001
     C                     MOVELINF,12    INFMSG                                              CER001
     C                     ENDIF                                                              CER001
     C           W0RTN     IFEQ 'Y2U9999'                                                     CER001
     C                     MOVE '01'      AID                                                 CER001
     C                     MOVEA'1'       *IN,10                                              CER001
     C                     ENDIF                                                              CER001
     C           W0RTN     IFEQ 'USR0790'                                                     CER001
     C                     MOVE '05'      AID                                                 CER001
     C                     ENDIF                                                              CER001
     C                     ENDIF                                                              CER001
      *                                                                                       CER001
     C                     ENDIF                                                              CER001
      *                                                                                       CER001
     C*
     C**FOR REC.ADVANCE DETERMINE WHAT SCREEN TO RETURN TO -  S T A R T
     C*
     C           W0RTN     IFNE 'USR0790'                                                     CER001
     C           W0RTN     ANDNE'Y2U9999'                                                     CER001
      *                                                                                       CER001
     C           REVW      IFEQ 'Y'
     C                     MOVELPPAGK     PAGK
     C                     MOVEA'1'       *IN,11
     C                     ELSE
     C                     MOVEA'1'       *IN,10
