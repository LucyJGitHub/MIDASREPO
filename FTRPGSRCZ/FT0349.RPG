     C                     MOVEL'FTM3244' FTMSID
     C                     EXSR FTSNMS
     C                     MOVEL'Y'       I7RCRM
     C                     MOVEL'Y'       E#NOUP
     C                     ENDIF                                                              200949
     C                     ENDIF
      *
     C                     ENDIF
