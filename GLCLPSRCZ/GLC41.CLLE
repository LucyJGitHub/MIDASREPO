                  &RTCD *NE '*NRF   ') THEN(DO)                       /*CEU024*/
/**********       GOTO       CMDLBL(ERROR)                         */              /*CEU024 CGL120*/
                  GOTO       CMDLBL(ABNOR)                                                /*CGL120*/
             ENDDO                                                    /*CEU024*/
