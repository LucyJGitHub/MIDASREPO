             CHGVAR     VAR(&SAR) VALUE('CMX001')                     /*CMX001*/
             CHGVAR     VAR(&CMX001) VALUE('N')                       /*CMX001*/
/**/                                                                  /*CMX001*/
/**  Check if Meridian export is installed                   */       /*CMX001*/
