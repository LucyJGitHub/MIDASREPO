                          *NE 'GZSDCURRPD') *AND (&GFGPF *NE +
                          'GZSDNOSTPD')) THEN(DO)                                         /*248348*/
                   CALL       PGM(GOC0100) PARM(&GFGPF)
                ENDDO
             ENDDO

             GOTO       CMDLBL(READNEXT)

