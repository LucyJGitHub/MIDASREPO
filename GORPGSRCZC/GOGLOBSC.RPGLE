/*STD *  RPGBASEMOD                                                   *
/*EXI *  TEXT('Midas GO Global updates - SC')                         *
      *****************************************************************
      *                                                               *
      *  Midas - Global Processing Module                             *
      *                                                               *
      *  GOGLOBSC - Global Updates - SC                               *
      *                                                               *
      *  (c) Finastra International Limited 2003                      *
      *                                                               *
      *  Last Amend No. MD056555           Date 31Aug20               *
      *  Prev Amend No. MD055681           Date 31Jul20               *
      *                 MD046248           Date 27Oct17               *
      *                 MD046901           Date 21Jul17               *
      *                 MD023043A          Date 27Nov13               *
      *                 CGP017             Date 06Aug12               *
      * Bank Fusion Midas 1.4 Base -----------------------------------*
      * Midas Plus 1.4 Base 04 ---------------------------------------*
      * Midas Plus 1.4 Base ------------------------------------------*
      * Midas Plus 1.3 ----------- Base ------------------------------*
      *                 CGP001  *CREATE    Date 23May03               *
      *                                                               *
      *---------------------------------------------------------------*
      *                                                               *
      *  MD056555 - Deliverable Data Split for SAR group              *
      *  MD055681 - Deliverable Data Split for SAR                    *
      *  MD046248 - Finastra Rebranding                               *
      *  MD046901 - Batch details listview is not loading in the      *
      *             other zone.                                       *
      *  MD023043A - Encounters an error upon switch-ON of CSW213     *
      *              (Recompile)                                      *
      *  CGP017 - Global Layer - GOC0100 Task Split (Recompile)       *
      *  CGP001 - Global Processing                                   *
      *                                                               *
      *****************************************************************

     D/COPY ZACPYSRC,STD_D_SPEC


     D PARM1           DS
      * Physical file name
     D  PFLNAME                1     10
      * Member name
     D  MBRNAME               21     30
      * Trigger event
     D  TRGEVENT              31     31
      * Offset to the before image of the record
     D  BOFF                  49     52B 0
      * Length of the before image of the record
     D  BLEN                  53     56B 0
      * Offset to the after image of the record
     D  AOFF                  65     68B 0
      * Length of the after image of the record
     D  ALEN                  69     72B 0
     D  EntryData            117  10117A


     D PARM2           DS
     D  LENG                   1      4B 0


      * ENTRY PARAMETERS

     C     *ENTRY        PLIST
     C                   PARM                    PARM1
     C                   PARM                    PARM2

      * Recover before and after image

     C     BOFF          ADD       1             OffSet            5 0
     C     BLen          SUBST     PARM1:OffSet  I#BIMG         4000

     C     AOFF          ADD       1             OffSet
     C     ALen          SUBST     PARM1:OffSet  I#AIMG         4000


      * Global Updates - BEFORE/AFTER image
      * TRGEVENT: '1'=INSERT, '2'=DELETE, '3'=UPDATE
     C     TRGEVENT      IFEQ      '1'
     C                   MOVEL     *BLANK        I#BIMG
     C                   ENDIF
     C     TRGEVENT      IFEQ      '2'
     C                   MOVEL     *BLANK        I#AIMG
     C                   ENDIF

     C                   MOVEL     O#ZONE        I#ZONE                         * ERROR MESSAGE
     C                   MOVEL     'Y'           I#TRAP

      * Determine updates

     C                   EXSR      DET_UPDATES

     C                   MOVEL     'B'           I#BFAF
     C                   DO        2

     C     P_Before      IFEQ      'Y'
     C     I#BFAF        ANDEQ     'B'
     C     P_After       OREQ      'Y'
     C     I#BFAF        ANDEQ     'A'
     C*****PFLNAME       CASEQ     'SCSARDPD  '  GLOB_SCSARDPD                              MD055681
     C     PFLNAME       CASEQ     'SCSRDCTD  '  GLOB_SCSARDPD                              MD055681
     C     PFLNAME       CASEQ     'SCSRDBTD  '  GLOB_SCSARDPD                              MD055681
     C     PFLNAME       CASEQ     'SCSRDXTD  '  GLOB_SCSARDPD                              MD055681
     C*****PFLNAME       CASEQ     'SCSAREPD  '  GLOB_SCSAREPD                              MD055681
     C*****PFLNAME       CASEQ     'SCSARIPD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRICTD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRIBTD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRIXTD  '  GLOB_SCSARIPD                              MD056555
     C*****PFLNAME       CASEQ     'SCSARWPD  '  GLOB_SCSARWPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRWCTD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRWBTD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCSRWXTD  '  GLOB_SCSARIPD                              MD056555
     C     PFLNAME       CASEQ     'SCOEAPTD  '  GLOB_SCOEAPTD                              MD046901
     C                   ENDCS
     C                   ENDIF

     C                   MOVEL     'A'           I#BFAF
     C                   ENDDO

     C                   RETURN

      /SPACE 5
      *****************************************************************
      * Global Updates - Switchable Features Descriptions
      *****************************************************************
     C     GLOB_SCSARDPD BEGSR
      /COPY GPCPYSRCG,GPSCSARDPD
     C                   ENDSR
      *****************************************************************
      /SPACE 5
      *****************************************************************
      **Global*Updates - Switchable Features Disabled                                       MD055681
      *****************************************************************
     C*****GLOB_SCSAREPD BEGSR                                                              MD055681
      *COPY*GPCPYSRCG,GPSCSAREPD                                                            MD055681
     C**********         ENDSR                                                              MD055681
      *****************************************************************
      /SPACE 5
      *****************************************************************
      * Global Updates - Switchable Features Menu Items
      *****************************************************************
     C     GLOB_SCSARIPD BEGSR
      /COPY GPCPYSRCG,GPSCSARIPD
     C                   ENDSR
      *****************************************************************
      /SPACE 5
      *****************************************************************
      * Global Updates - Switchable Features Pulldown Values
      *****************************************************************
     C     GLOB_SCSARWPD BEGSR
      /COPY GPCPYSRCG,GPSCSARWPD
     C                   ENDSR
      *****************************************************************                     MD046901
      /SPACE 5                                                                              MD046901
      *****************************************************************                     MD046901
      * Global Updates - Overridable Errors for Approval                                    MD046901
      *****************************************************************                     MD046901
     C     GLOB_SCOEAPTD BEGSR                                                              MD046901
      /COPY GPCPYSRCG,GPSCOEAPTD                                                            MD046901
     C                   ENDSR                                                              MD046901
      *****************************************************************
      /SPACE 5
      *****************************************************************
      * Determine Updates
      *****************************************************************
     C     DET_UPDATES   BEGSR

     C                   CALL      'GPDETGUPD'
     C                   PARM                    I#ZONE           10
     C                   PARM      PFLNAME       I#FILE           10
     C                   PARM                    I#BIMG
     C                   PARM                    I#AIMG
     C                   PARM      'N'           P_Before          1
     C                   PARM      'N'           P_After           1

     C                   ENDSR
      *****************************************************************
     C/SPACE 5
      ********************************************************************
      * *INZSR - Initial subroutine called automatically at program start
      ********************************************************************
     C     *INZSR        BEGSR

     C                   MOVEL     *BLANK        I#RTCD            7
     C                   MOVEL     *BLANK        I#ERMS           50

      * Get Zone

      /COPY GOCPYSRC,GOGETZONE

     C                   ENDSR
      ********************************************************************
      /SPACE 5
