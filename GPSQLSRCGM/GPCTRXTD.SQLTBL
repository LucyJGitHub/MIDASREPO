   A4XUSR TEXT IS 'Authorise User' ,                                            :
   A4ZONE TEXT IS 'Zone' ,                                                      :
   A4ETXY TEXT IS 'End of Year Tax' ,                                           :
   A4RPFQ TEXT IS 'Reporting Frequency' ,                                       :
   A4TXCY TEXT IS 'Currency of Tax' ,                                           :
   A4JAMG TEXT IS 'Joint Acc. Management' ,                                     :
