   A4CTWC IS 'Weighting           Code' ,                                       :
   A4LCD IS 'Last                Change              Date' ,                    :
   A4CHTP IS 'Change              Type' ,                                       :
   A4CUSR IS 'Change              User' ,                                       :
   A4ISOC IS 'ISO Country         code' ,                                       :
   A4IUSR IS 'Insert User' ,                                                    :
   A4XUSR IS 'Authorise           User' ,                                       :
