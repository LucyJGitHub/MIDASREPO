                                                                                :
  create table                                                                  :
  **********/**********                                                         :
  (                                                                             :
    GROBJ CHAR(10) NOT NULL DEFAULT ''                                          :
  , GRTYPE CHAR(8) NOT NULL DEFAULT ''                                          :
  , GRMODE CHAR(1) NOT NULL DEFAULT ''                                          :
  )                                                                             :
                                                                                :
  RCDFMT GPGRSXT0  ;                                                            :
                                                                                :
  LABEL ON TABLE **********/**********                                          :
  IS 'Midas GP Global Objects Restored in PEA System Ext';                      :
                                                                                :
