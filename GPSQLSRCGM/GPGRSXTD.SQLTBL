  (                                                                             :
   GROBJ IS 'Object' ,                                                          :
   GRTYPE IS 'Type'  ,                                                          :
   GRMODE IS 'Mode'                                                             :
  ) ;                                                                           :
                                                                                :
  LABEL ON COLUMN **********/**********                                         :
  (                                                                             :
   GROBJ TEXT IS 'Object' ,                                                     :
   GRTYPE TEXT IS 'Type'  ,                                                     :
   GRMODE TEXT IS 'Mode'                                                        :
  );                                                                            :
