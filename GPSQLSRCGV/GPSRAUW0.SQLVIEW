                                                                                :
  create view                                                                   :
  **********/**********                                                         :
  (                                                                             :
    SASARN                                                                      :
  , SAOBJ                                                                       :
  , SATYPE                                                                      :
  , SADEPN                                                                      :
  , SADEL                                                                       :
  )                                                                             :
  as                                                                            :
  select                                                                        :
       c.SASARN                                                                 :
     , c.SAOBJ                                                                  :
     , c.SATYPE                                                                 :
     , c.SADEPN                                                                 :
     , c.SADEL                                                                  :
  from GPSRACTD c                                                               :
                                                                                :
  union all                                                                     :
  select                                                                        :
       b.SASARN                                                                 :
     , b.SAOBJ                                                                  :
     , b.SATYPE                                                                 :
     , b.SADEPN                                                                 :
     , b.SADEL                                                                  :
  from GPSRABTD b                                                               :
                                                                                :
  rcdfmt GPSRAUT0;                                                              :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas SC 24x7 Zonal Database Switchable Union File';                     :
