                                                                                :
  create view                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       MGID                                                                     :
     , MGHYID                                                                   :
     , MGNAME                                                                   :
     , MGDELETED                                                                :
     , MGMODE                                                                   :
  )                                                                             :
  as select                                                                     :
       x.MGID                                                                   :
     , x.MGHYID                                                                 :
     , x.MGNAME                                                                 :
     , x.MGDELETED                                                              :
     , u.MGMODE                                                                 :
  from T_MNGUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  T_MNGXTD x                                                                    :
  on                                                                            :
  u.MGID = x.MGID                                                               :
                                                                                :
  rcdfmt T_MNGJT0;                                                              :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas GP Menu subgroup                       Joint';                     :
