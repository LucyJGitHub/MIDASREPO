  CREATE VIEW                                                                   :
  **********/**********                                                         :
  (                                                                             :
   MLLIMITID ,                                                                  :
   MLCCY ,                                                                      :
   MLAMT ,                                                                      :
   MLREMARK ,                                                                   :
   MSWID ,                                                                      :
   MSWSTAT ,                                                                    :
   MSWNAVP ,                                                                    :
   MSWCAT ,                                                                     :
   MSWINPUSER ,                                                                 :
   MSWINPTS ,                                                                   :
   MSWAUTHTS ,                                                                  :
   MSWAPPR ,                                                                    :
***MSWAUTH )                                                                    :            BUG3344
   MSWAUTH,                                                                     :            BUG3344
   LITYPE, LIUTSRTYPE, LIUTSRCODE,                                              :            BUG3344
   LIENTYTYPE, LIENTYCODE, LIINSTGRP,                                           :            BUG3344
   LITENOR)                                                                     :            BUG3344
   AS                                                                           :
   SELECT                                                                       :
     T_MLMLUEXT.*,                                                              :
     T_MSWIP.MSWSTAT,                                                           :
     T_MSWIP.MSWNAVP,                                                           :
     T_MSWIP.MSWCAT,                                                            :
     T_MSWIP.MSWINPUSER,                                                        :
     T_MSWIP.MSWINPTS,                                                          :
     T_MSWIP.MSWAUTHTS,                                                         :
     T_MSWIP.MSWAPPR,                                                           :
*****T_MSWIP.MSWAUTH                                                            :            BUG3344
*****FROM T_MLMLUEXT, T_MSWIP                                                   :            BUG3344
*****WHERE T_MLMLUEXT.MSWID = T_MSWIP.MSWID ;                                   :            BUG3344
     T_MSWIP.MSWAUTH,                                                           :            BUG3344
     T_MLLIMIT.LITYPE, T_MLLIMIT.LIUTSRTYPE, T_MLLIMIT.LIUTSRCODE,              :            BUG3344
     T_MLLIMIT.LIENTYTYPE, T_MLLIMIT.LIENTYCODE,  T_MLLIMIT.LIINSTGRP,          :            BUG3344
     T_MLLIMIT.LITENOR                                                          :            BUG3344
     FROM T_MLMLUEXT INNER JOIN T_MSWIP                                         :            BUG3344
          ON T_MLMLUEXT.MSWID = T_MSWIP.MSWID                                   :            BUG3344
          INNER JOIN T_MLLIMIT ON MLLIMITID=LILIMITID;                          :            BUG3344
