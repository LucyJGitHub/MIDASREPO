  CREATE VIEW                                                                   :
  **********/**********                                                         :
   AS                                                                           :
   SELECT                                                                       :
     MTID, CAST(MTID AS CHAR(2)) AS PADMTID, MTREF, MTMODULE, MTTRNTYP,         :
     MTTRNSTYP, MTBRCHCODE, MTCUSTCODE, MTASSETAMT, MTASSETCCY, MTLIABAMT ,     :
     MTLIABCCY, MTDEALDATE, MTVALDATE, MTMATDATE, MTREMARK, MTRESVID,           :
     MTDELETED, MTUSERID, MTFACBRCD, MTFACCUST, MTFACTYPE, MTFACSEQ             :
   FROM T_MLMTRAN WHERE MTDELETED <> 'Y' ;                                      :
