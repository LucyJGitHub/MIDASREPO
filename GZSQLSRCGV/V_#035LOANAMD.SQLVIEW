***CASE*WHEN*(C.PTFC*=*0*AND*C.PTFT*=*0*AND*C.PTFN*=*0)*THEN*F.SYIN*************: /*CLE106*/ BG12361
   CASE WHEN ((C.PTFC = '' AND C.PTFT = 0 AND C.PTFN = 0) OR                                 BG12361
   (C.PTFC = '000000' AND C.PTFT = 0 AND C.PTFN = 0)) THEN F.SYIN                            BG12361
   ELSE Z.SYIN END AS SYIN                                                      : /*CLE106*/
***,CASE*WHEN*(C.PTFC*=*0*AND*C.PTFT*=*0*AND*C.PTFN*=*0)*THEN*F.PRTR************: /*CLE106*/ BG12361
   ,CASE WHEN ((C.PTFC = '' AND C.PTFT = 0 AND C.PTFN = 0) OR                   :            BG12361
   (C.PTFC = '000000' AND C.PTFT = 0 AND C.PTFN = 0)) THEN F.PRTR               :            BG12361
   ELSE Z.PRTR END AS PRTR,                                                     : /*CLE106*/
***DIGITS(L.CNUM)*AS*PADCNUM,*DIGITS(L.FCUS)*AS*PADFCUS,************************: /*CLE106*/  CSD027
          L.CNUM  AS PADCNUM,        L.FCUS  AS PADFCUS,                        :             CSD027
   DIGITS(L.FTYP) AS PADFTYP, DIGITS(LASN) AS PADLASN,                          : /*CLE106*/
***DIGITS(L.FSEQ)*AS*PADFSEQ,*DIGITS(L.LNRF)*AS*PADLNRF,************************: /*CLE106*/  CLE148
   DIGITS(L.FSEQ) AS PADFSEQ,        L.LNRF  AS PADLNRF,                        :             CLE148
   DIGITS(SETP) AS PADSETP, DIGITS(L.RSTM) AS PADRSTM,                          : /*CLE106*/
   DIGITS(L.PSTM) AS PADPSTM,                                                   : /*CLE106*/
   CASE WHEN L.VDAT=0 THEN NULL ELSE DATE(L.VDAT+719892) END AS FVDAT,          : /*CLE106*/
   CASE WHEN LSMD=0 THEN NULL ELSE DATE(LSMD+719892) END AS FLSMD,              :
***CASE*WHEN*ORED=0*THEN*NULL*ELSE*DATE(ORED+719892)*END*AS*FORED,**************: /*CLE106*/
   CASE WHEN L.ORED=0 THEN NULL ELSE DATE(L.ORED+719892) END AS FORED,          : /*CLE106*/
***CASE*WHEN*LCD=0*THEN*NULL*ELSE*DATE(LCD+719892)*END*AS*FLCD,*****************: /*CLE106*/
   CASE WHEN L.LCD=0 THEN NULL ELSE DATE(L.LCD+719892) END AS FLCD,             : /*CLE106*/
   CASE WHEN XAVD=0 THEN NULL ELSE DATE(XAVD+719892) END AS FXAVD,              :BUG907
