***<=*F6.BJRDNB*then*CAST('S'*as*char(1))*ELSE*CAST(F1.RECI*AS*CHAR(1))*********:             248070
   <= F6.BJRDNB then CAST('S' as char(1))                                       :             248070
       WHEN F1.TDFS=0 AND F1.RECI='*' then CAST('C' as char(1))                 :             248070
       WHEN F1.TDFS<>0 AND F1.RECI='*' then CAST('S' as char(1))                :             248070
       WHEN F1.TDFS = F6.BJRDNB then CAST('S' as char(1))                       :          AR1040642
       WHEN F1.RECI = 'A' THEN CAST('D' AS CHAR(1))                             :          AR1077605
       WHEN (F1.RECI ='L' OR F1.RECI ='P') THEN CAST('R' AS CHAR(1))            :          AR1077605
       ELSE CAST(F1.RECI as char(1))                                            :             248070
   END as NEWRECI,                                                              :
   A8ZONE AS CUSTZONE,                                                          :
