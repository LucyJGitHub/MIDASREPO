***F1.TMST,*F1.ORDE,*F1.REPI,*F1.AUTH,*F1.BWDI,*F1.CRSK,*F1.LQPR,***************:          AR1041752
   F1.TMST,                                                                     :          AR1041752
   CASE WHEN F1.ORDE = 999999 OR F1.ORDE = 0 THEN ''                            :          AR1041752
        ELSE DIGITS(F1.ORDE) END AS ORDE,                                       :          AR1041752
   F1.REPI, F1.AUTH, F1.BWDI, F1.CRSK, F1.LQPR,                                 :          AR1041752
   F1.TINN, F1.ITRN, F1.PRICN, F2.SPBS, F2.STBS,                                :
   F2.SRPN, F2.SFN1, F2.SFN2,  F2.SREF, F2.T2RF, F2.CSPN,                       :            BUG2256
   F2.ISIN, F2.CPNR,                                                            :            BUG2256
   CASE WHEN F2.MATY=0 THEN NULL ELSE DATE(F2.MATY+719892) END AS FMATY,        :            BUG2256
   F3.PROT, F4.C1DI, F5.BVAUTH,                                                 :
   F1.TDVD,F1.TDDT,F1.TDFS,F1.TDMC,F1.TOED,F1.LCD,F2.MATY,                      :BUG6107
***DIGITS(F1.TCNR)*as*PADTCNR,**************************************************:             CSD027
***CASE*WHEN*F1.DELT=0*THEN*''*ELSE*DIGITS(F1.DELT)*END*as*PADDELT,*************:             CSD027
***CASE*WHEN*F1.DELF=0*THEN*''*ELSE*DIGITS(F1.DELF)*END*as*PADDELF,*************:             CSD027
          F1.TCNR  as PADTCNR,                                                  :             CSD027
   CASE WHEN F1.DELT='' THEN '' ELSE        F1.DELT  END as PADDELT,            :             CSD027
   CASE WHEN F1.DELF='' THEN '' ELSE        F1.DELF  END as PADDELF,            :             CSD027
   CASE WHEN F3.PROT<>'8' THEN NULL ELSE F1.TCFC END AS FTCFC,                  :
   CASE WHEN F1.ASNM=0 THEN '' ELSE DIGITS(F1.ASNM) END as PADASNM,             :
   CASE WHEN F1.TCAP=0 THEN NULL ELSE F1.TCAP END as FTCAP,                     :
   DIGITS(F1.SMTH) as PADSMTH,                                                  :
   F1.NOML/POWER(10,F1.TNMD) as SCALEDNOML,                                     :
   F1.TOSN/POWER(10,F1.TNMD) as SCALEDTOSN,                                     :
   F1.TNSN/POWER(10,F1.TNMD) as SCALEDTNSN,                                     :
   F1.TNSP/POWER(10,F1.TNMD) as SCALEDTNSP,                                     :
***CASE*WHEN*F1.ORDE=0*THEN*''*ELSE*DIGITS(F1.ORDE)*END*AS*PADORDE,*************:          AR1041752
   CASE WHEN F1.ORDE = 0 OR F1.ORDE = 999999                                    :          AR1041752
   THEN '' ELSE DIGITS(F1.ORDE) END AS PADORDE,                                 :          AR1041752
   CASE WHEN F1.SPMD=' ' THEN NULL ELSE F1.SPXR END AS FSPXR,                   :
   CASE WHEN F1.TDVD=0 THEN NULL ELSE DATE(F1.TDVD+719892) END AS FTDVD,        :
   CASE WHEN F1.TDDT=0 THEN NULL ELSE DATE(F1.TDDT+719892) END AS FTDDT,        :
   CASE WHEN F1.TDFS=0 THEN NULL ELSE DATE(F1.TDFS+719892) END AS FTDFS,        :
   CASE WHEN F1.TDMC=0 THEN NULL ELSE DATE(F1.TDMC+719892) END AS FTDMC,        :
   CASE WHEN F1.TOED=0 THEN NULL ELSE DATE(F1.TOED+719892) END AS FTOED,        :
   CASE WHEN F1.LCD=0 THEN NULL ELSE DATE(F1.LCD+719892) END AS FLCD,           :
