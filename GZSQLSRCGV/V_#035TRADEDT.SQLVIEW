***DEC(F1.TVSN*C2.SCALING,31,4) AS "STVSN",*************************************:BUG5305
***DEC(F1.TVSP*C2.SCALING,31,4) AS "STVSP",*************************************:BUG5305
***DEC(F1.TDSN*C1.SCALING,31,4) AS "STDSN",*************************************:BUG5305
***DEC(F1.TDSP*C1.SCALING,31,4) AS "STDSP",*************************************:BUG5305
***DEC(F1.TCSR*C1.SCALING,31,4) AS "STCSR",*************************************:BUG5305
***DEC(F1.TCTR*C1.SCALING,31,4) AS "STCTR",*************************************:BUG5305
***DEC(F1.TCTR*C1.SCALING*C1.RATE,31,4) AS "BSTCTR",****************************:BUG5305
***DEC(F1.MAMT*C1.SCALING,31,4) AS "SMAMT",*************************************:BUG5305
