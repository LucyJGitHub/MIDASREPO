  CREATE VIEW                                                                    :
  **********/**********                                                          :
  AS                                                                             :
  SELECT                                                                         :
     RECI, BRCA, BOKC, ISTT, YRNO, MTHN, PCAL, STRP, TRTY,                       :
     ULTT, TRSD, NUCO, NOCO, NOBO, COPR, TNAR, TBLO, ECPI,                       :
