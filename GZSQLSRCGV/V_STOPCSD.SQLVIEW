  CREATE VIEW                                                                   :
  **********/**********                                                         :
  AS                                                                            :
**SELECT************************************************************************:             CAP201
**FROM*V_#STOPCSD***************************************************************:             CAP201
  SELECT V_#STOPCSD.*,                                                          :             CAP201
     (SELECT COUNT(*) FROM DLSTCQX0 EX                                          :             CAP201
     WHERE EX.CNUM = V_#STOPCSD.CNUM                                            :             CAP201
        AND EX.CCY = V_#STOPCSD.CCY                                             :             CAP201
        AND EX.ACOD = V_#STOPCSD.ACOD                                           :             CAP201
        AND EX.ACSQ = V_#STOPCSD.ACSQ                                           :             CAP201
        AND EX.STCR = V_#STOPCSD.STCR                                           :             CAP201
********AND*EX.TNLU*=*V_#STOPCSD.TNLU*******************************************:    CAP201 BUG27045
        AND EX.BRCA = V_#STOPCSD.BRCA                                           :             CAP201
********AND*EX.CUSTZONE*=*V_#STOPCSD.CUSTZONE)**********************************:    CAP201 BUG27045
        AND EX.CUSTZONE = V_#STOPCSD.CUSTZONE                                   :           BUG27045
********AND*EX.TMSTP*=*V_#STOPCSD.TMSTP)****************************************: BUG27045 BUG27045A
        AND EX.INSTMP = V_#STOPCSD.INSTMP)                                      :          BUG27045A
     AS PCLIP                                                                   :             CAP201
     FROM V_#STOPCSD  V_#STOPCSD                                                :             CAP201
