  CREATE VIEW                                                                   :
  **********/**********                                                         :
  (MICORRPK,                                                                    :
   MIFCOR,                                                                      :
   MICUST,                                                                      :
   MIADD1,                                                                      :
   MIADD2,                                                                      :
   MIADD3,                                                                      :
   MIADD4,                                                                      :
   MIZIP,                                                                       :
   VERSIONNUM)                                                                  :
  AS                                                                            :
  SELECT DISTINCT CDCORR, CAFCOR,                                               :
  CDCUST, CDADD1, CDADD2, CDADD3, CDADD4,                                       :
  CDZIP, 1 AS VERSIONNUM FROM CGCASCPD                                          :
  LEFT JOIN CGCORRPD ON CATCOR=CDCORR                                           :
  OR CAFCOR=CDCORR;                                                             :
/*******************************************************************************/
 ;                                                                              :
  label on table     **********/**********                                      :
   is 'Midas HIL Associate correspondent view            ';                     :
