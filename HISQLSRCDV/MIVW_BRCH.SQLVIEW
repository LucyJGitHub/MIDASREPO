  LABEL ON COLUMN **********/**********                                         :
  (  MIBRCDPK TEXT IS 'Branch Code',                                            :
     MIBRNM TEXT IS 'Branch Name',                                              :
     MIBICN TEXT IS 'Branch Internal Customer Number',                          :
     MICMCD TEXT IS 'Company Code',                                             :
     MILCCD TEXT IS 'Location Code',                                            :
     MILCD TEXT IS 'Last Change Date',                                          :
     MILCDS TEXT IS 'Last Change Date (SQL)',                                   :
     MITYLC TEXT IS 'Type of Last Change',                                      :
     MIGBCD TEXT IS 'Global Branch Code',                                       :
     MIBRSN TEXT IS 'Branch Shortname',                                         :
