  LABEL ON COLUMN **********/**********                                         :
   ( MIBRCDPK IS 'Branch Code',                                                 :
     MIBRNM IS 'Branch Name',                                                   :
     MIBICN IS 'Branch Internal Customer Number',                               :
     MICMCD IS 'Company Code',                                                  :
     MILCCD IS 'Location Code',                                                 :
     MILCD IS 'Last Change Date',                                               :
