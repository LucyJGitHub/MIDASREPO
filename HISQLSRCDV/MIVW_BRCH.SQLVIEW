     MICUSR TEXT IS 'Change User',                                              :
     MIZONE TEXT IS 'Zone',                                                     :
     MIDFAC TEXT IS 'Due From Account code',                                    :
     MIDTAC TEXT IS 'Due To Account code',                                      :
     MIIUSR TEXT IS 'Insert User',                                              :
     MIXUSR TEXT IS 'Authorise User',                                           :
     MIAACC TEXT IS 'Automatically accept w/in zone',                           :
     MIEACC TEXT IS 'Accept by Effective Date',                                 :
     MIEACC TEXT IS 'Accept by Effective Date (SQL)',                           :
     MICDIN TEXT IS 'Central Data indicator',                                   :
