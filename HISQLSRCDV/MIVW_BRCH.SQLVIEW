     A8DTAC,                                                                    :
     A8IUSR,                                                                    :
     A8XUSR,                                                                    :
     A8AACC,                                                                    :
     A8EACC,                                                                    :
     CASE WHEN A8EACC=0 THEN NULL ELSE DATE(A8EACC+719892) END AS A8EACS,       :
     A8GPDT,                                                                    :
     1                                                                          :
  FROM SDBRCHPD                                                                 :
  RCDFMT MIVW_BRCH ;                                                            :
