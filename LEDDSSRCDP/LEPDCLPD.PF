     A            DDLOAM        14A         TEXT('Loan Outstanding Principal')
     A                                      COLHDG('Loan' 'Outstanding' +
     A                                             'Principal')
     A            DDAIPD        14A         TEXT('Loan Accrued Interest')
     A                                      COLHDG('Loan Accrued' 'Interest')
