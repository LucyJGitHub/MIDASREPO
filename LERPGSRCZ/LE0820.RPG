0807 CSR                   SETOF                     949599               LE0820
0808 C*                                                                   LE0820
0809 C*    DETERMINE SIGN OF ZZAMTI & D,   92 IF NEG                      LE0820
0810 CSR         ZZAMTI    COMP 0                      9293               LE0820
0811 CSR 93      ZZAMTD    COMP 0                      9293               LE0820
0812 CSR 93                GOTO ZZSEND                     ZERO BYPASS    LE0820
0813 C*                                                                   LE0820
0814 C*    DETERMINE SIGN OF ZZTOTI & D, 91 IF NEG.                       LE0820
0815 CSR         ZZTOTI    COMP 0                      9193               LE0820
0816 CSR 93      ZZTOTD    COMP 0                      9193               LE0820
0817 C*                                                                   LE0820
0818 C*    IF ZZTOTAL IS ZERO, RETURN ZZAMOUNT.                           LE0820
0819 CSR 93                Z-ADDZZAMTI    ZZTOTI                          LE0820
0820 CSR 93                Z-ADDZZAMTD    ZZTOTD                          LE0820
0821 CSR 93                GOTO ZZSEND                     ZERO BYPASS    LE0820
0822 C*    IF SIGNS DIFFER BYPASS OVERFLOW CHECKS.                        LE0820
0823 CSR 91N92                                                            LE0820
0824 CORN91 92             GOTO ZZOFPS                                    LE0820
0825 C*                                                                   LE0820
0826 CSR         ZZAMTD    ADD  ZZTOTD    ZZWK2   40                      LE0820
0827 CSR         ZZWK2     COMP 999                  93    '93' = CARRY   LE0820
0828 CSRN93      ZZWK2     COMP -999                   93    INTO INTEGER.LE0820
0829 C*                                                                   LE0820
0830 CSR 93N92   ZZAMTI    ADD  1         ZZWK3  150       ADD 'CARRY' +VELE0820
0831 CSR    93 92ZZAMTI    SUB  1         ZZWK3            SUB 'CARRY' -VELE0820
0832 CSR 93      ZZTOTI    ADD  ZZWK3     ZZWK3                           LE0820
0833 CSRN93      ZZTOTI    ADD  ZZAMTI    ZZWK3                           LE0820
0834 C*                                                                   LE0820
0835 C* IF THE MODULUS OF ZZWK3 IS LT MOD. ZZTOTI THEN O/F HAS OCCURED    LE0820
0836 CSRN92      ZZWK3     COMP ZZTOTI                 99  -92 MEANS NOS. LE0820
0837 CSR 92      ZZWK3     COMP ZZTOTI               99     NEGATIVE      LE0820
0838 CSRN99                Z-ADDZZWK2     ZZTOTD                          LE0820
0839 CSRN99                Z-ADDZZWK3     ZZTOTI                          LE0820
0840 C*                                                                   LE0820
0841 C* IF O/F ZEROIZE ZZAMT , IND '99' SET AND ZZTOT FIELDS LEFT INTACT. LE0820
0842 CSR 99                Z-ADD0         ZZAMT  153                      LE0820
0843 CSR                   GOTO ZZSEND                                    LE0820
0844 C*                                                                   LE0820
0845 C* THE 'SIGNS' ARE DIFFERENT                                         LE0820
0846 CSR         ZZOFPS    TAG                             ** ZZOFPS TAG**LE0820
0847 C* IF ZZAMT WAS NEGATIVE, MAKE IT POS. TO COMP WITH ZZTOT            LE0820
0848 C*                                                                   LE0820
0849 CSR 92                Z-SUBZZAMTI    ZZAMTI 150                      LE0820
0850 CSR 92                Z-SUBZZAMTD    ZZAMTD  30                      LE0820
0851 C*                                                                   LE0820
0852 C* IF ZZTOT WAS NEGATIVE, MAKE IT POS. TO COMP WITH ZZAMT.           LE0820
0853 C*                                                                   LE0820
0854 CSR 91                Z-SUBZZTOTI    ZZTOTI                          LE0820
0855 CSR 91                Z-SUBZZTOTD    ZZTOTD                          LE0820
0856 C* BOTH ZZAMT AND ZZTOT ARE NOW POSITIVE.                            LE0820
0857 C*                                                                   LE0820
0858 CSR         ZZTOTI    COMP ZZAMTI               93  95               LE0820
0859 CSR 95      ZZTOTD    COMP ZZAMTD               93  95               LE0820
0860 C*                                                                   LE0820
0861 C* IF ZZTOT EQ. ZZAMT RETURN ZERO.                                   LE0820
0862 CSR 95                Z-ADD0         ZZTOTI                          LE0820
0863 CSR 95                Z-ADD0         ZZTOTD                          LE0820
0864 CSR 95                GOTO ZZSEND                                    LE0820
0865 C*                                                                   LE0820
0866 C* IF ZZTOT GT. ZZAMT.                                               LE0820
0867 CSR 93      ZZAMTD    COMP ZZTOTD               94                   LE0820
0868 CSR 93 94   ZZTOTI    SUB  1         ZZTOTI                          LE0820
0869 CSR 93 94   ZZTOTD    ADD  1000      ZZWK2                           LE0820
0870 CSR 93      ZZTOTI    SUB  ZZAMTI    ZZTOTI                          LE0820
0871 CSR 93 94   ZZWK2     SUB  ZZAMTD    ZZTOTD                          LE0820
0872 CSR 93N94   ZZTOTD    SUB  ZZAMTD    ZZTOTD                          LE0820
0873 C*                                                                   LE0820
0874 C*                                                                   LE0820
0875 C* IF ZZAMT GT. ZZTOT.                                               LE0820
0876 CSRN93      ZZTOTD    COMP ZZAMTD               94                   LE0820
0877 CSRN93 94   ZZAMTI    SUB  1         ZZWK3  150                      LE0820
0878 CSRN93 94   ZZAMTD    ADD  1000      ZZWK2                           LE0820
0879 CSRN93 94   ZZWK3     SUB  ZZTOTI    ZZTOTI                          LE0820
0880 CSRN93N94   ZZAMTI    SUB  ZZTOTI    ZZTOTI                          LE0820
0881 CSRN93 94   ZZWK2     SUB  ZZTOTD    ZZTOTD                          LE0820
0882 CSRN93N94   ZZAMTD    SUB  ZZTOTD    ZZTOTD                          LE0820
0883 C*                                                                   LE0820
0884 C* REVERSE SIGN OF ZZTOT IF LARGER I/P FIELDS WERE NEGATIVE          LE0820
0885 CSR                   SETOF                     94                   LE0820
0886 CSR 93 91                                                            LE0820
0887 CORN93 92             SETON                     94                   LE0820
0888 CSR 94                Z-SUBZZTOTI    ZZTOTI                          LE0820
0889 CSR 94                Z-SUBZZTOTD    ZZTOTD                          LE0820
0890 C**                                                                  LE0820
0891 C**   RESTORE SIGN OF ZZAMTI & ZZAMTD IF IT WAS REVERSED.            LE0820
0892 CSR 92                Z-SUBZZAMTI    ZZAMTI                          LE0820
0893 CSR 92                Z-SUBZZAMTD    ZZAMTD                          LE0820
0894 CSR         ZZSEND    TAG                             ** ZZSEND TAG *LE0820
0895 C**                                                                  LE0820
0896 C**   IF ZZTOTD IS ZERO, AND ZZTOTI IS NEG. SET UP ZZNEGD.           LE0820
0897 CSR                   SETOF                     96                   LE0820
0898 CSR         ZZTOTD    COMP 0                        91               LE0820
0899 CSR 91      ZZTOTI    COMP 0                      96                 LE0820
0900 CSR 96                MOVE '.000-'   ZZNEGD  5                       LE0820
0901 CSR                   ENDSR                             ** ENDSR **  LE0820
