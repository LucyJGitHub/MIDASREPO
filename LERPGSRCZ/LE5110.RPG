     C           REPT      IFEQ 1
     C           REPT      OREQ 3
     C           NRPD      IFEQ 0
     C                     Z-ADDMDAT      TODAT   50
     C                     ELSE
     C                     Z-ADDNRPD      TODAT
     C                     ENDIF
     C                     ELSE
     C           NIDT      IFEQ 0
     C                     Z-ADDMDAT      TODAT
     C                     ELSE
     C           NIDT      IFLT MDAT
     C                     Z-ADDNIDT      TODAT
     C                     ELSE
     C                     Z-ADDMDAT      TODAT
     C                     ENDIF
     C                     ENDIF
     C                     ENDIF
      *
     C           NIDT      IFGT MDAT
