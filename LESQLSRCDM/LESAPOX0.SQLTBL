     DDACOD, DDACSQ, J1DAID));                                                  :
                                                                                :
  LABEL ON COLUMN LESAPOX0                                                      :
   (DDBRCA TEXT IS 'Branch',                                                    :
    DDCNUM TEXT IS 'Customer Number',                                           :
    DDCCY TEXT IS 'Currency',                                                   :
    DDACOD TEXT IS 'Account Code',                                              :
    DDACSQ TEXT IS 'Account Sequence',                                          :
    J1DAID TEXT IS 'DL PRIMARY KEY');                                           :
