     ,SEFSEQ is 'Fee Sequence'                                                  :
     ,SEFCOD is 'Fee Code'                                                      :
     ,SEAKEY is 'Account Key'                                                   :
     ,SEVDAT is 'Value Date'                                                    :
     ,SEREVI is 'Reversal Indicator'                                            :
