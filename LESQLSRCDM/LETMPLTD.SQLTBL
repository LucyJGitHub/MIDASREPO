     ,ACCOUNT_CODE for column SEACOD numeric(10, 0) with default 0              :
     ,ACCOUNT_SEQUENCE for column SEACSQ numeric(2, 0) with default 0           :
     ,FACILITY_CUSTOMER for column SEFCUS char(6) with default ' '              :
     ,FACILITY_CODE for column SEFACL char(5) with default ' '                  :
     ,LOAN_REFERENCE for column SELNRF char(6) with default ' '                 :
     ,FEE_SEQUENCE for column SEFSEQ char(2) with default ' '                   :
     ,FEE_CODE for column SEFCOD char(2) with default ' '                       :
     ,ACCOUNT_KEY for column SEAKEY char(10) with default ' '                   :
     ,VALUE_DATE for column SEVDAT numeric(5,0) with default 0                  :
     ,REVERSAL_INDICATOR for column SEREVI numeric (1,0) with default 0         :
     ,OVERDRAFT_INDICATOR for column SEOVFL char(1) default ' '                 :
     ,CATEGORY_CODE for column SECATC char(5) with default ' '                  :
     ,REPAYMENT_PRIORITY for column SEREPR numeric(8,0) with default 0          :
     ,CATEGORY_CODE_PRIORITY for column SECAPR numeric(8,0) with default 0      :
