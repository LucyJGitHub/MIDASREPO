     ,MATURITY_DATE for column SEMDAT decimal(5, 0) with default 0              :
     ,VALUE_DATE_PDCL for column SEVDPD decimal(5, 0) with default 0            :
     ,VALUE_DATE_PAYMENT for column SEVDPY decimal(5, 0) with default 0         :
     ,PAYMENT_AMOUNT for column SEPAMT decimal(15, 0) with default 0            :
     ,TIMESTAMP for column SETMST timestamp                                     :
     ,RETAIL_ACCOUNT for column SEACNO numeric(10,0) with default 0             :
     ,CLEARED_BALANCE for column SECLBL decimal(15, 0) with default 0           :
     ,AVAILABLE_AMOUNT for column SEAAMT decimal(15, 0) with default 0          :
     ,OVERDRAFT_AMOUNT for column SEOAMT decimal(15, 0) with default 0          :
