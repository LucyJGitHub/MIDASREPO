     ,PROCESSS_FLAG for column SEIPFL char(1) with default 'N'                  :
     ,SEARCH_KEY  for column SEKEYF char(24) with default ' '                   :
     ,INDEX_1 for column SEIND1 numeric(8, 0) with default 0                    :
     ,INDEX_2 for column SEIND2 numeric(8, 0) with default 0                    :
     ,AVAILABLE_BLANCE_FLAG for column SEAVFL char(1) with default 'N'          :
     ,BLOCK_DEBIT_FLAG for column SERETB char(1) with default 'N'               :
     ,CAP205_FLAG for column SEC205 char(1) with default 'N'                    :
     ,ABC_FLAG for column SEABCF char(1) with default 'N'                       :
     ,ALT_ACCOUNT for column SEALTA char (1) with default 'N'                   :
     ,ALT_ACSEQ for column SEALTSQ char (3) with default ' '                    :
   );                                                                           :
                                                                                :
  label on table **********/**********                                          :
  is 'LE Combined Extract File';                                                :
