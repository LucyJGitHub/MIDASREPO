     ,SEAVOD is 'Available Balance + Overdraft'                                 :
     ,SEHELD is 'Held Items'                                                    :
     ,SECOLL is 'Collateral'                                                    :
     ,SEEAMT is 'Event Amount'                                                  :
     ,SERAMT is 'Repayment Amount'                                              :
     ,SEIPFL is 'Processed Flag'                                                :
     ,SEKEYF is 'Search Key'                                                    :
     ,SEIND1 is 'Index 1'                                                       :
     ,SEIND2 is 'Index 2'                                                       :
     ,SEAVFL is 'Available Balance Indicator'                                   :
     ,SERETB is 'Block All Debits Indicator'                                    :
     ,SEC205 is 'CAP205 Indicator'                                              :
     ,SEABCF is 'ABC Flag'                                                      :
     ,SEALTA is 'Alternative Account Indicator'                                 :
     ,SEALTSQ is 'Alternative Account Sequence'                                 :
   );                                                                           :
 ;                                                                              :
  label on table     **********/**********                                      :
   is 'Midas LE Combined Extract Files Template Table    ';                     :
