       THEN DIGITS(CDFTYP) ELSE '' END                                          :
  ,CASE WHEN GLCOLLPD.CDGNSP = 'S'                                              :
       THEN GLCOLLPD.CDFSEQ ELSE 0 END                                          :
  ,CASE WHEN GLCOLLPD.CDGNSP = 'S' AND GLCOLLPD.CDFSEQ <> 0                     :
       THEN DIGITS(CDFSEQ) ELSE '' END                                          :
