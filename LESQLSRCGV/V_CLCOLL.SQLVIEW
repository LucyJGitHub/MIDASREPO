  SELECT DISTINCT                                                               :
  GLCOLLPD.CDZONE                                                               :
  ,CASE WHEN GLCOLLPD.CDRECI ='D' THEN 'ACTIVE'                                 :
       WHEN GLCOLLPD.CDRECI ='M' THEN 'MATURED'                                 :
       ELSE 'DELETED' END                                                       :
**,CASE*WHEN*GLCOLLPD.CDGNSP*=*'S'**********************************************:           BUG22525
*******THEN*GLCOLLPD.CDFCUS*ELSE 0*END******************************************:           BUG22525
  ,CASE WHEN GLCOLLPD.CDGNSP = 'S' AND GLCOLLPD.CDFCUS <> ''                    :           BUG22525
       THEN GLCOLLPD.CDFCUS ELSE '' END                                         :          BUG22525A
*******THEN*GLCOLLPD.CDFCUS*ELSE*0*END******************************************: BUG22525 BUG22525A
**,CASE*WHEN*GLCOLLPD.CDGNSP*=*'S'*AND*GLCOLLPD.CDFCUS*<>*0*********************:             CSD027
*******THEN*DIGITS(CDFCUS)*ELSE*''*END******************************************:             CSD027
  ,CASE WHEN GLCOLLPD.CDGNSP = 'S' AND GLCOLLPD.CDFCUS <> ''                    :             CSD027
       THEN        CDFCUS  ELSE '' END                                          :             CSD027
