  FROM                                                                          :
  GZSDBANKPD SDBANKPD                                                           :
  ,GZGLCOLLPD GLCOLLPD                                                          :
  LEFT OUTER JOIN                                                               :
  GZGLCOLAPD GLCOLAPD                                                           :
  ON                                                                            :
  GLCOLLPD.CDCREF = GLCOLAPD.CACREF                                             :
  AND GLCOLLPD.CDZONE = GLCOLAPD.CAZONE                                         :
  ,GZSDCOTPPD SDCOTPPD                                                          :
  ,GZSDCURRPD SDCURRPD                                                          :
  WHERE                                                                         :
  SDBANKPD.BJZONE = GLCOLLPD.CDZONE                                             :
  AND GLCOLLPD.CDCOLT = SDCOTPPD.CTCOLT                                         :
  AND GLCOLLPD.CDZONE = SDCOTPPD.CTZONE                                         :
  AND GLCOLLPD.CDCCY =SDCURRPD.A6CYCD                                           :
  AND GLCOLLPD.CDZONE = SDCURRPD.A6ZONE;                                        :
