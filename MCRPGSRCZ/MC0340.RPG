     C                     MOVE '1'       *IN10
     C                     MOVE '1'       *IN13
     C                     Z-ADDSFNUM3    SFNUMX
     C                     MOVE SFILE3    SFILEX
     C                     END
      *
     C                     ENDSL
      *
      **  BRCHD - Process Change of Branch
      *
     C           *INL5     IFEQ '1'
     C                     EXSR BRCHD
     C                     END
      *
      **  CURRD - Process change in Currency
      *
     C           *INL4     IFEQ '1'
     C                     EXSR CURRD
     C                     END
