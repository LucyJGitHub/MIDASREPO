   STMTPY IS 'Message             Type' ,                                       :
   STMTAG IS 'Message             Tag' ,                                        :
   STVALF IS 'Validation          Flag' ,                                       :
   STMODE IS 'Mode'                                                             :
  ) ;                                                                           :
                                                                                :
  LABEL ON COLUMN **********/**********                                         :
  (                                                                             :
   STMTPY TEXT IS 'Message type' ,                                              :
   STMTAG TEXT IS 'Message Tag' ,                                               :
   STVALF TEXT IS 'Validation Flag' ,                                           :
   STMODE TEXT IS 'Mode'                                                        :
  );                                                                            :
