    STMTPY CHAR(3) NOT NULL DEFAULT ''                                          :
  , STMTAG CHAR(5) NOT NULL DEFAULT ''                                          :
  , STVALF CHAR(8) NOT NULL DEFAULT ''                                          :
  , STMODE CHAR(1) NOT NULL DEFAULT ''                                          :
