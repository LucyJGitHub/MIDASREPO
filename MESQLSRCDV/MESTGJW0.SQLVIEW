                                                                                :
  create view                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       STMTPY                                                                   :
     , STMTAG                                                                   :
     , STVALF                                                                   :
     , STMSQN                                                                   :
     , STMODE                                                                   :
  )                                                                             :
  as select                                                                     :
       x.STMTPY                                                                 :
     , x.STMTAG                                                                 :
     , x.STVALF                                                                 :
     , u.STMSQN                                                                 :
     , u.STMODE                                                                 :
  from MESTGUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  MESTGXTD x                                                                    :
  on                                                                            :
  u.STMTPY = x.STMTPY and u.STMTAG = x.STMTAG and u.STVALF = x.STVALF           :
                                                                                :
  rcdfmt MESTGJT0;                                                              :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas Starting Message Tag in Sequence       Joint';                     :
