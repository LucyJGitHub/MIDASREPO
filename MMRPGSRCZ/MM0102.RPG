     C                     Z-ADDETXR1     ULWHTR                                              CDL013
     C           TAXI      WHEQ '2'                                                           CDL013
     C                     Z-ADDETXR2     ULWHTR                                              CDL013
     C           TAXI      WHEQ '3'                                                           CDL013
