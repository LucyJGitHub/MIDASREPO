             IF         COND((&CSW027 *EQ 'Y') *AND (&CSW052 *EQ +
                          'Y')) THEN(GOTO CMDLBL(LOOPIFS))                                /*CSW052*/
             GOTO       CMDLBL(LOOP)
 
