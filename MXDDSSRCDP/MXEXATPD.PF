/*EXI *  TEXT('Midas MX Meridian Export Attributes Format')
      *****************************************************************
      *                                                               *
      *  Midas - Meridian Export Module                               *
      *                                                               *
      *  PF/MXEXATPD - Meridian Export Attributes Format              *
      *                                                               *
      *  (c) Misys International Banking Systems Ltd. 2001            *
      *                                                               *
      * Bank Fusion Midas 1.4 Base -----------------------------------*
      * Midas Plus 1.4 Base 04 ---------------------------------------*
      * Midas Plus 1.4 Base ------------------------------------------*
      * Midas Plus 1.3 ----------- Base ------------------------------*
      * Midas Release 4 --------------- Base -------------------------*
      * Midas DBA 3.05 -----------------------------------------------*
      *  Last Amend No. 189210             Data 25Jan01               *
      *  Prev Amend No. CMX001  *CREATE    Data 01Jan00               *
      *                                                               *
      *---------------------------------------------------------------*
      *                                                               *
      *  189210 - Add additional dates to Export Attributes format    *
      *  CMX001 - Meridian Export                                     *
      *                                                               *
      *****************************************************************
     A                                      REF(MFRFL)
     A          R MXEXATP0                  TEXT('(c) Misys International +
     A                                      Banking Systems Ltd. +
     A                                      2001')
     A            EAEXTT         1A
     A                                      TEXT('Extract Type')
                                            COLHDG('Extract' 'Type')
     A            EAMODE         1A
     A                                      TEXT('Mode')
                                            COLHDG('Mode')
     A            EAMRDT         7A
     A                                      TEXT('Midas Run Date')
                                            COLHDG('Midas' 'Run' 'Date')
     A            EARDNB         5S 0
     A                                      TEXT('Midas Run Day Number')
                                            COLHDG('Midas' 'Run' 'Dayno')
     A            EADNWD         5S 0
     A                                      TEXT('Next Working Day Day Number')
                                            COLHDG('Nxt' 'Wrk' 'Dayno')
     A            EABANC1        4
     A                                      TEXT('Branch Analysis Code 1')
                                            COLHDG('Branch' 'Analysis' '1')
     A            EABANC2        4
     A                                      TEXT('Branch Analysis Code 2')
                                            COLHDG('Branch' 'Analysis' '2')
     A            EACANC1        4
     A                                      TEXT('Composite Analysis Code 1')
                                            COLHDG('Com' 'Analysis' '1')
     A            EACANC2        4
     A                                      TEXT('Composite Analysis Code 2')
                                            COLHDG('Com' 'Analysis' '2')
     A            EABCCY         3A
     A                                      TEXT('Base Currency')
                                            COLHDG('Base' 'Currency')
     A            EABCDP         1S 0
     A                                      TEXT('Base Currency Dec. Places')
                                            COLHDG('Base' 'Ccy' 'D.P.')
     A*                                                                      /*Start 189210*/
     A            EACEOM         5S 0
     A                                      TEXT('Current End of Month')
                                            COLHDG('Curr' 'EOM')
     A            EACSOM         5S 0
     A                                      TEXT('Current Start of Month')
                                            COLHDG('Curr' 'SOM')
     A            EAPSOM         5S 0
     A                                      TEXT('Previous Start of Month')
                                            COLHDG('Prev' 'SOM')
     A*                                                                      /* End  189210*/
