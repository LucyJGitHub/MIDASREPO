     C           DDTYPD    ANDEQ' '                           3
     C           DDMAXR    ORNE ' '                           3
     C           DDGRPD    ANDEQ' '                           3
     C           DDTYPD    ANDEQ' '                           3
     C                     MOVE '1'       *IN75               3
     C                     MOVE '1'       *IN36               3
