     C                     MOVELWWPTIN    ZMDINF
     C                     MOVELWWNOIN    ZMDINT
     C                     Z-ADDWWPTDP    ZCDPF
     C                     Z-ADDWWNODP    ZCDPT
     C                     Z-ADDFLFXPL    ZAMTF
     C                     EXSR ZCCYCN
     C                     Z-ADDZAMTT     WWFXPL 150
