     C           *IN16     OREQ '1'
     C           WRBRCA    ANDNEFRTLBC
     C           CRT001    ANDEQ'Y'
     C           CRT003    ANDEQ'Y'
     C           A8BRST    ANDNE'N'
      *
     C           *IN16     OREQ '0'
