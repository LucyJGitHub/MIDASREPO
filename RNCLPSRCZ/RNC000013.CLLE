             PGM        PARM(&ERROR &FILE)
 
             DCL        VAR(&ERROR) TYPE(*LGL)
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(10)
             DCLF       FILE(QTEMP/QAFDMBRL)
 
             COPYRIGHT  TEXT('(c) Finastra International Limited +
                          2005')
 
/* Global monitor message */
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(ABNOR))
 
             CHKOBJ     OBJ(&FILE) OBJTYPE(*FILE) MBR(*FIRST)
             MONMSG     MSGID(CPF9815) EXEC(GOTO CMDLBL(ENDPGM))
 
             DSPFD      FILE(&FILE) TYPE(*MBRLIST) OUTPUT(*OUTFILE) +
                          OUTFILE(QTEMP/QAFDMBRL)
 
/* Read through list and clear member. */
 RCVF:
             RCVF
             MONMSG     MSGID(CPF0864) EXEC(GOTO CMDLBL(ENDPGM))
 
             CLRPFM     FILE(&MLFILE) MBR(&MLNAME)
             GOTO       CMDLBL(RCVF)
 
 ABNOR:
             CHGVAR     VAR(&ERROR) VALUE('1')
 
 ENDPGM:
             ENDPGM
