     c     *pssr         begsr
 
     c                   if        not inPssr
     c                   eval      inPssr      = *on
     c                   eval      *inu7       = *on
     c                   eval      *inu8       = *on
 
     c     *lock         in        lda
     c                   eval      dbpgm       = PSProcPgm
     c                   eval      dbfile      = wDbfile
     c                   eval      dbkey       = wDbkey
     c                   eval      dbase       = wDbase
     c                   eval      dbmod       = wDbmod
     c                   eval      dbproc      = wDbproc
     c                   out       lda
 
     c                   dump
     c                   callb     'DBERRCTL'
     c                   exsr      closedown
 
     c                   else
     c                   eval      *inlr       = *on
     c                   return
     c                   endif
 
     c                   endsr
