                ((&SCC_SASARN = 'CAP735') *AND (&CAP735 = 'Y')) *OR +
                ((&SCC_SASARN = 'CAP773') *AND (&CAP773 = 'Y')) *OR +
                ((&SCC_SASARN = 'CAP774') *AND (&CAP774 = 'Y')) *OR +
                ((&SCC_SASARN = 'CAP702') *AND (&CAP702 = 'Y'))) THEN(DO) /*CAP773*/

             IF         COND((&SCC_SATYPE *EQ 'LF') *AND +
                          (&SCC_SADEPN *EQ 'DM')) THEN(DO)
                MOVOBJ     OBJ(&DVLIB/&SCC_SAOBJ) OBJTYPE(*FILE) +
                             TOLIB(&DMLIB)
                          GOTO LOOP1
             ENDDO

             IF         COND((&SCC_SATYPE *EQ 'LF') *AND +
                          (&SCC_SADEPN *EQ 'DP')) THEN(DO)
                MOVOBJ     OBJ(&DVLIB/&SCC_SAOBJ) OBJTYPE(*FILE) +
                             TOLIB(&DPLIB)
                          GOTO LOOP1
             ENDDO

             IF         COND((&SCC_SATYPE *EQ 'LF') *AND (&SCC_SADEPN +
                          *EQ 'DT')) THEN(DO)
             MOVOBJ     OBJ(&DVLIB/&SCC_SAOBJ) OBJTYPE(*FILE) +
                             TOLIB(&DTLIB)
                          GOTO LOOP1
             ENDDO

             IF         COND((&SCC_SATYPE *EQ 'LF') *AND (&SCC_SADEPN +
                          *EQ 'DV')) THEN(DO)              /*CAP773*/
                          GOTO LOOP1                       /*CAP773*/
             ENDDO       /*CAP773*/
                          GOTO LOOP1                       /*MD057749*/
                ENDDO       /*CAP734*/
                          GOTO LOOP1                       /*MD057749*/
