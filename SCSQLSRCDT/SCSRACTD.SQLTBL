                                                                                 :
  CREATE TABLE                                                                   :
  **********/**********                                                          :
  (                                                                              :
    SASARN CHAR(6) NOT NULL                                                      :
  , SAOBJ CHAR(10) NOT NULL                                                      :
  , SATYPE CHAR(10) NOT NULL                                                     :
  , SADEPN CHAR(2) NOT NULL                                                      :
  , SASARD CHAR(50) NOT NULL                                                     :
  , SAMODE CHAR(1) NOT NULL DEFAULT 'C'                                          :
  , SADEL CHAR(1) NOT NULL DEFAULT 'N')                                          :
                                                                                 :
  RCDFMT SCSRACT0   ;                                                            :
                                                                                 :
  LABEL ON TABLE **********/**********                                           :
  IS 'MIDAS SC 24X7 ZONAL DATABASE SWITCHABLE CORE FILE' ;                       :
                                                                                 :
  LABEL ON COLUMN **********/**********                                          :
  (SASARN IS 'SWITCHABLE FEATURE NUMBER' ,                                       :
  SAOBJ IS '24X7 DATABASE OBJECT' ,                                              :
  SATYPE IS 'OBJECT TYPE' ,                                                      :
  SADEPN IS 'OBJECT DEPENDENCY' ,                                                :
  SASARD IS 'DESCRIPTION' ,                                                      :
  SAMODE IS 'CORE OR NON CORE' ,                                                 :
  SADEL IS 'LOGICALLY DELETED' ) ;                                               :
                                                                                 :
  LABEL ON COLUMN **********/**********                                          :
  (SASARN TEXT IS 'SWITCHABLE FEATURE NUMBER' ,                                  :
  SAOBJ TEXT IS '24X7 DATABASE OBJECT' ,                                         :
  SATYPE TEXT IS 'OBJECT TYPE' ,                                                 :
  SADEPN TEXT IS 'OBJECT DEPENDENCY' ,                                           :
  SASARD TEXT IS 'DESCRIPTION' ,                                                 :
  SAMODE TEXT IS 'CORE OR NON CORE' ,                                            :
  SADEL TEXT IS 'LOGICALLY DELETED' ) ;                                          :
