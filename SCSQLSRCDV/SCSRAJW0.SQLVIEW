/******************************************************************************/
                                                                                :
  CREATE VIEW                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       SASARN                                                                   :
     , SAOBJ                                                                    :
     , SATYPE                                                                   :
     , SADEPN                                                                   :
     , SADEL                                                                    :
  )                                                                             :
  AS SELECT                                                                     :
       U.SASARN                                                                 :
     , U.SAOBJ                                                                  :
     , U.SATYPE                                                                 :
     , U.SADEPN                                                                 :
     , U.SADEL                                                                  :
  FROM SCSRAUW0 U                                                               :
                                                                                :
  JOIN                                                                          :
                                                                                :
  SCSRAXTD X                                                                    :
  ON                                                                            :
      U.SASARN = X.SASARN AND U.SAOBJ = X.SAOBJ                                 :
  RCDFMT SCSRAJT0;                                                              :
                                                                                :
  LABEL ON TABLE     **********/**********                                      :
   IS 'MIDAS SC 24X7 ZONAL DATABASE SWITCHABLE JOIN FILE';                      :
