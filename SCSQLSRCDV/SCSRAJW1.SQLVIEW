                                                                                :
  CREATE VIEW                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       SASARN                                                                   :
     , SAOBJ                                                                    :
     , SATYPE                                                                   :
     , SADEPN                                                                   :
     , SADEL                                                                    :
  )                                                                             :
  AS SELECT                                                                     :
       U.SASARN                                                                 :
     , U.SAOBJ                                                                  :
     , U.SATYPE                                                                 :
     , U.SADEPN                                                                 :
     , U.SADEL                                                                  :
**FROM*SCSRAJW0*U***************************************************************:           MD057254
  FROM SCSRAJW2 U                                                               :           MD057254
                                                                                :
**LEFT*OUTER*JOIN*SCSARDPD*W*ON*(*U.SASARN*=*W.SARN*)***************************:           MD057254
  LEFT OUTER JOIN SCSRDXTD W ON ( U.SASARN = W.SARN )                           :
  WHERE U.SADEL = 'N'                                                           :
  AND W.ENABLD = 'E'                                                                        MD057254
  ;                                                                             :
  LABEL ON TABLE     **********/**********                                      :
   IS 'MIDAS SC 24X7 ZONAL DATABASE SWITCHABLE JOIN FILE';                      :
