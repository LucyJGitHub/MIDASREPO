                                                                                 :
  create table                                                                   :
  **********/**********                                                          :
  (                                                                              :
    JFNAME CHAR(50) NOT NULL DEFAULT ''                                          :
  , JFMBR CHAR(10) NOT NULL DEFAULT ''                                           :
  , JFMODE CHAR(1) NOT NULL DEFAULT ''                                           :
  )                                                                              :
                                                                                 :
  RCDFMT SDJARXT0  ;                                                             :
                                                                                 :
  LABEL ON TABLE **********/**********                                           :
  IS 'Midas SD JAR Files Extension                    ';                         :
                                                                                 :
  LABEL ON COLUMN **********/**********                                          :
  (                                                                              :
  JFNAME IS 'FILE NAME' ,                                                        :
  JFMBR IS 'Member of' ,                                                         :
  JFMODE IS 'Mode'                                                               :
  ) ;                                                                            :
                                                                                 :
  LABEL ON COLUMN **********/**********                                          :
  (                                                                              :
  JFNAME TEXT IS 'FILE NAME' ,                                                   :
  JFMBR TEXT IS 'Member of' ,                                                    :
  JFMODE TEXT IS 'Mode'                                                          :
  );                                                                             :
