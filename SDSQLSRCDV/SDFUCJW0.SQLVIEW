  as select                                                                     :
       x.ININTF                                                                 :
     , x.INUSER                                                                 :
     , u.INUNIQ                                                                 :
     , x.INUSRD                                                                 :
     , x.INTMST                                                                 :
     , x.INTYLC                                                                 :
     , x.INLCUS                                                                 :
     , x.INMODE                                                                 :
  from SDFUCUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  SDFUCXTD x                                                                    :
  on                                                                            :
      u.INUNIQ = x.INUNIQ                                                       :
  rcdfmt SDFUCJT0;                                                              :
