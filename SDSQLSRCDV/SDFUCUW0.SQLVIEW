     b.ININTF                                                                   :
   , b.INUSER                                                                   :
   , b.INUNIQ                                                                   :
   , b.INUSRD                                                                   :
   , b.INTMST                                                                   :
   , b.INTYLC                                                                   :
   , b.INLCUS                                                                   :
