                                                                                :
  create view                                                                   :
  **********/**********                                                         :
  (                                                                             :
    JFNAME                                                                      :
  , JFMBR                                                                       :
  , JFMODE                                                                      :
  )                                                                             :
  as                                                                            :
  select                                                                        :
       c.JFNAME                                                                 :
     , c.JFMBR                                                                  :
     , c.JFMODE                                                                 :
  from SDJARCTD c                                                               :
                                                                                :
  union all                                                                     :
  select                                                                        :
       b.JFNAME                                                                 :
     , b.JFMBR                                                                  :
     , b.JFMODE                                                                 :
  from SDJARBTD b                                                               :
                                                                                :
  rcdfmt SDJARUT0 ;                                                             :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas SD JAR Files Union                        ';                       :
