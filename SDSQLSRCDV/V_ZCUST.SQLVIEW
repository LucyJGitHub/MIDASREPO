/*BBMGTP,*BBNET,*BBNTGP,*BBNTCP,*BBNTND,*BBNTMM,*BBTICS,*BBSWFI,*BBDINC,********:             BG1289
  BBMGTP, BBNET, BBNTGP, BBNTCP, BBNTND, BBNTMM, BBTICS, BBDINC,                :             BG1289
  BBCOMG, BBCHRG, BBAGNT, BBAGNP, BBTRAS, BBNAR1, BBNAR2, BBNAR3, BBNAR4,       :
  BBDFAC, BBDFAS, BBFRNT, BBAFRT, BBREPA, BBTMST, BBPRBA, BBCLSS,               :
  BBCLST, BBCLDT, BBCORP, BBCBEI, BBRI01, BBRI02, BBRI03, BBRI04, BBRI05,       :
  BBFUND,                                                                       :
                                                                                :
  CASE WHEN BBSSDT=0 THEN NULL ELSE DATE(BBSSDT+719892) END AS FBBSSDT,         :
  CASE WHEN BBDOIC=0 THEN NULL ELSE DATE(BBDOIC+719892) END AS FBBDOIC,         :
  CASE WHEN BBLCD=0 THEN NULL ELSE DATE(BBLCD+719892) END AS FBBLCD,            :
  CASE WHEN BBLALD=0 THEN NULL ELSE DATE(BBLALD+719892) END AS FBBLALD,         :
  CASE WHEN BBLDHD=0 THEN NULL ELSE DATE(BBLDHD+719892) END AS FBBLDHD,         :
  CASE WHEN BBLRLD=0 THEN NULL ELSE DATE(BBLRLD+719892) END AS FBBLRLD,         :
  CASE WHEN BBDTDL=0 THEN NULL ELSE DATE(BBDTDL+719892) END AS FBBDTDL,         :
  CASE WHEN BBORED=0 THEN NULL ELSE DATE(BBORED+719892) END AS FBBORED,         :
  CASE WHEN BBEFDT=0 THEN NULL ELSE DATE(BBEFDT+719892) END AS FBBEFDT,         :
/*CASE*WHEN*BBDTDL>0*THEN*'C'*ELSE*'L'*END*AS*STATUS****************************:             BG1289
  CASE WHEN BBDTDL>0 THEN 'C' ELSE 'L' END AS STATUS,                           :             BG1289
  CASE WHEN BBSWFI = 'Y' THEN 'Y' ELSE 'N' END AS NEWBBSWFI                     :             BG1289
                                                                                :
  FROM SDCUSTPD                                                                 :
