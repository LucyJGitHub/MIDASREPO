             PGM
/**/
/*/COPY WNCPYSRC,SEC0101002                                          */
             DCL        VAR(&CPYFLD) TYPE(*CHAR) LEN(64) VALUE('(c) +
                          Misys International Banking Systems Ltd. +
                          2001')
/*/COPY WNCPYSRC,SEC0101003                                          */
/**/                                                                  /*S01117*/
             SNDPGMMSG  MSG('Copy Start of Business Trades & Security +
                          Events') TOMSGQ(MRUNQ)
/**/
             CHGJOB     SWS(00000000)
/**/
             CLRPFM     FILE(ATPRPD)                                  /*S01271*/
/**/
             CPYF       FROMFILE(TRADSD) TOFILE(SOBTRD) +
                          MBROPT(*REPLACE)
/************MONMSG*****MSGID(CPF2817)*EXEC(CLRPFM FILE(SOBTRD))****/ /*E17328*/
             MONMSG     (CPF2817) CMPDTA(CPF2869)          +
                        EXEC(CLRPFM SOBTRD)                           /*E17659*/
/**/                                                                  /*CSE015*/
/************CPYF       FROMFILE(DPTMVD) TOFILE(SOBDMVD) +                             */ /*CPK018*/
/************             MBROPT(*REPLACE)                                  */ /*CSE015*/ /*CPK018*/
             CPYF       FROMFILE(DPTMVD) TOFILE(SOBDMVD) +
                          MBROPT(*REPLACE) FMTOPT(*MAP *DROP)                             /*CPK018*/
             MONMSG     (CPF2817) CMPDTA(CPF2869)        +
                        EXEC(CLRPFM SOBDMVD)                          /*CSE015*/
/**/                                                                  /*CSE015*/
/*/COPY WNCPYSRC,SEC0101001                                          */
/**/
             CPYF       FROMFILE(SEDEVD) TOFILE(SOBSDD) +
                          MBROPT(*REPLACE)
/************MONMSG*****MSGID(CPF2817)*EXEC(CLRPFM FILE(SOBSDD))****/ /*E17328*/
             MONMSG     (CPF2817) CMPDTA(CPF2869)          +
                        EXEC(CLRPFM SOBSDD)                           /*E17659*/
/**/
             CPYF       FROMFILE(SNDEVD) TOFILE(SOBSND) +
                          MBROPT(*REPLACE)
/************MONMSG*****MSGID(CPF2817)*EXEC(CLRPFM FILE(SOBSND))****/ /*E17328*/
             MONMSG     (CPF2817) CMPDTA(CPF2869)          +
                        EXEC(CLRPFM SOBSND)                           /*E17659*/
             CPYF       FROMFILE(SDCSINPD) TOFILE(SOBCSINPD) MBROPT(*REPLACE)             /*235615*/
             MONMSG     MSGID(CPF2817) CMPDTA(CPF2869) EXEC(CLRPFM FILE(SOBCSINPD))       /*235615*/
/**/
             CHGVAR     VAR(&CPYFLD) VALUE('(c) +
                          Misys International Banking Systems Ltd.')
             ENDPGM
