     C                     Z-ADDNMDP      ZDECS
     C                     MOVE 'J'       ZECODE
     C                     EXSR ZSEDIT
     C                     MOVE ZOUT21    RTCVR
     C                     MOVE ZOUT21    RTCSR                           E21518
     C*
     C**  Print total lines
     C*
     C                     WRITETOTAL                  52
     C                     ADD  7         LCNT
     C*
     C           *IN21     IFEQ '1'
     C           *IN22     OREQ '1'
     C                     ADD  1         LCNT
     C                     END
     C*
     C**  Initialise fields
     C*
     C                     Z-ADD*ZEROS    TCNT
     C                     Z-ADD*ZEROS    TCNV
     C                     Z-ADD*ZEROS    TCNS
     C                     Z-ADD*ZEROS    TCTR
     C                     Z-ADD*ZEROS    TCVR
     C*
     C**  Print headings if overflow + side headings for next group
     C**                if same Branch/Security
     C*
     C           *IN90     IFEQ '0'
     C***********DCBR      ANDEQSAVBR                                     S01117
     C           DCBA      ANDEQSAVBR                                     S01117
     C           DCSS      ANDEQSESN
     C           LCNT      IFGE 50
     C                     WRITEHEAD1
     C                     WRITEHEAD2
     C                     Z-ADD9         LCNT
     C                     END
     C*
     C           DCDP      IFEQ 'D'
     C                     SETON                     20
     C                     SETOF                     2122
     C                     WRITEHEAD3
     C                     ADD  4         LCNT
     C                     ELSE
     C           *IN20     IFEQ '1'
     C                     WRITEHEAD4
     C                     ADD  3         LCNT
     C                     ELSE
     C                     WRITEHEAD3
     C                     ADD  4         LCNT
     C                     END
     C                     END
     C                     END
     C*
     C                     ENDSR
