     C           *IN01     DOWEQ'0'
     C           *INU7     ANDEQ'0'
     C*
     C           DDPMV     IFEQ 'DT'
     C                     EXSR USRTRF
     C                     END
     C*
     C           DDPMV     IFEQ 'WI'
     C           DDPMV     OREQ 'WO'
     C                     EXSR WALKIO
     C                     END
     C*
     C           DDPMV     IFEQ 'RP'
     C           DDPMV     OREQ 'RR'
     C           DDPMV     OREQ 'BB'
     C           DDPMV     OREQ 'BL'
     C           DDPMV     OREQ 'PD'
     C           DDPMV     OREQ 'PL'
     C                     EXSR REPO
     C                     END
     C*
     C**  Write to OTHSTD audit so that in the event of failure the
     C**  OTHST FILE and its audit trailer match
     C*
     C           *INU7     IFEQ '0'
     C           1         CHAINOTHSTA               03
     C                     Z-ADDOTHCT     NORE
     C                     UPDATOTHSTAF                03
     C           *IN03     IFEQ '1'
     C                     SETON                     U7
     C                     END
     C                     END
     C*
     C**  IF OK then update the depot movement
     C**            read next Depot Movement
     C*
     C           *INU7     IFEQ '0'
     C                     EXCPTDEPOTP
     C                     END
     C*
     C           *INU7     IFEQ '0'
     C                     READ DPTMVDF                  01
     C                     END
     C*
     C**  END while loop
     C*
