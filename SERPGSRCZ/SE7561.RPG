     C                     ELSE
     C                     MOVE *BLANKS   WACTST           (9 - 10)
     C                     ENDIF
     C                     MOVE *BLANKS   WACCCC           (11 - 12)
     C                     MOVE WOMKTI    WACMKT           (13)
      *
     C           BVCIAK    IFEQ 'Y'
     C           WOSCVI    ANDEQ'Y'
     C                     MOVE WOSCVI    WACCON           (14)
     C                     ELSE
     C                     MOVE *BLANKS   WACCON           (14)
     C                     ENDIF
      *
     C           BVNPAC    IFEQ 'N'
     C           WWEVAM    ORGE *ZERO
     C                     MOVE *BLANKS   WACNEG           (15)
     C                     ELSE
     C                     MOVE 'N'       WACNEG           (15)
     C                     ENDIF
