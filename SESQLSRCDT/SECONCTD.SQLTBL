                                                                                :
  RCDFMT SECONCT0  ;                                                            :
                                                                                :
  LABEL ON TABLE **********/**********                                          :
  IS 'Midas SE Linked Enquiry Control File          Core';                      :
                                                                                :
  LABEL ON COLUMN **********/**********                                         :
  (                                                                             :
   FROM IS 'From                Pgm                 Name' ,                     :
   OPTN IS 'Opt                 For                 Enq' ,                      :
   TPGM IS 'Called              Pgm for             Enquiry'  ,                 :
   FRMODE IS 'Mode'                                                             :
  ) ;                                                                           :
                                                                                :
  LABEL ON COLUMN **********/**********                                         :
  (                                                                             :
   FROM TEXT IS 'Calling program for linked enquiries' ,                        :
   OPTN TEXT IS 'Option selected for linked enquires' ,                         :
   TPGM TEXT IS 'Called program for Linked Enquiries'  ,                        :
   FRMODE TEXT IS 'Mode'                                                        :
  );                                                                            :
