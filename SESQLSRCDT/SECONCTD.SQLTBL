                                                                                :
  create table                                                                  :
  **********/**********                                                         :
  (                                                                             :
    FROM CHAR(8) NOT NULL DEFAULT ''                                            :
  , OPTN CHAR(2) NOT NULL DEFAULT ''                                            :
  , TPGM CHAR(8) NOT NULL DEFAULT ''                                            :
  , FRMODE CHAR(1) NOT NULL DEFAULT ''                                          :
