             PGM

             DCL        VAR(&CPYFLD) TYPE(*CHAR) LEN(64) VALUE('(c) +
                          Finastra International Limited 2021')
             DCL        VAR(&TOTREC) TYPE(*DEC) LEN(10 0)

/** Global monitor message */

             MONMSG     MSGID(CPF0000 MCH0000 RPG0000) EXEC(GOTO +
                          CMDLBL(ABNOR))

/** Update SDCUAHPD file and generate the update report **/

             CALL       PGM(SMU90004)
             IF         COND(%SWITCH(XXXXXX11)) THEN(GOTO +
                        CMDLBL(ABNOR))

             GOTO       CMDLBL(END)

 ABNOR:
             SNDPGMMSG  MSG('Program SMUC90004 ended abnormally - see joblog') +
                          TOPGMQ(*EXT) TOMSGQ(MOPERQ MRUNQ)
             MONMSG     MSGID(CPF0000 MCH0000 RPG0000)

 END:
             CHGVAR     VAR(&CPYFLD) VALUE('(c) Finastra +
                          International Limited')

             ENDPGM
