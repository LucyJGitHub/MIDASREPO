                                                                                :
  create view                                                                   :
  **********/**********                                                         :
                                                                                :
  (                                                                             :
       NCOBJ                                                                    :
     , NCTYPE                                                                   :
     , NCETBL                                                                   :
     , NCZFLD                                                                   :
     , NCMODE                                                                   :
  )                                                                             :
  as select                                                                     :
       x.NCOBJ                                                                  :
     , x.NCTYPE                                                                 :
     , u.NCETBL                                                                 :
     , u.NCZFLD                                                                 :
     , u.NCMODE                                                                 :
  from SMGNMUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  SMGNMXTD x                                                                    :
  on                                                                            :
  u.NCOBJ = x.NCOBJ and u.NCTYPE = x.NCTYPE                                     :
                                                                                :
  rcdfmt SMGNMJT0;                                                              :
                                                                                :
  label on table     **********/**********                                      :
   is 'Midas SM Global objects not to be migrated   Joint';                     :
