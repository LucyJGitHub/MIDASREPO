                                                                                :
  create view                                                                   :
  **********/**********                                                         :
  (                                                                             :
    NCOBJ                                                                       :
  , NCTYPE                                                                      :
  , NCETBL                                                                      :
  , NCZFLD                                                                      :
  , NCMODE                                                                      :
  )                                                                             :
  as                                                                            :
