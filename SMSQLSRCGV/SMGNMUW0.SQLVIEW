       c.NCOBJ                                                                  :
     , c.NCTYPE                                                                 :
     , c.NCETBL                                                                 :
     , c.NCZFLD                                                                 :
     , c.NCMODE                                                                 :
  from SMGNMCTD c                                                               :
                                                                                :
  union all                                                                     :
  select                                                                        :
       b.NCOBJ                                                                  :
     , b.NCTYPE                                                                 :
     , b.NCETBL                                                                 :
     , b.NCZFLD                                                                 :
     , b.NCMODE                                                                 :
  from SMGNMBTD b                                                               :
