      u.IFOBJ  = x.IFOBJ                                                        :
  and u.IFTYPE = x.IFTYPE                                                       :
  and u.IFSEQ  = x.IFSEQ                                                        :
                                                                                :
  rcdfmt QINZJW0;                                                               :
