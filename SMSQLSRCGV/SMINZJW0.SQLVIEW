  )                                                                             :
  as select                                                                     :
     u.IFOBJ                                                                    :
   , u.IFTYPE                                                                   :
   , u.IFSEQ                                                                    :
                                                                                :
   , u.IFMODE                                                                   :
                                                                                :
   , x.IFCMD                                                                    :
   , x.IFDEL                                                                    :
  from SMINZUW0 u                                                               :
                                                                                :
  join                                                                          :
                                                                                :
  SMINZXTD x                                                                    :
