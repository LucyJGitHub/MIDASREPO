  SUBSTR(A8BTID,1,4) AS CASWB,SUBSTR(A8BTID,5,2) AS CACNAS,                     :
  SUBSTR(A8BTID,7,2) AS CASWL, SUBSTR(A8BTID,10,3) AS CASWBR,                   :
