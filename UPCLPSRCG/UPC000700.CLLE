             MONMSG     MSGID(CPF9801) EXEC(GOTO CMDLBL(END))                           /*MD054955*/
             RMVLIBLE   LIB(&LIBMOV)                                                    /*MD054955*/
             MONMSG     MSGID(CPF0000)                                                  /*MD054955*/
             ADDLIBLE   LIB(&LIBMOV)                                                    /*MD054955*/
             CALL       PGM(UP000703) PARM(&ERROR &FILE +
