                                                                                        /*MD055265*/
/* FCRSFCTD */                                                                          /*MD055265*/
             IF         COND(&FILE *EQ 'FCRSFCTD') THEN(DO)                             /*MD055265*/
             CHGVAR     VAR(&LIBMOV) VALUE('#.' *TCAT %SUBSTRING(&OLDLIB 1 2) +
