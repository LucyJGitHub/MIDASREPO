             IF         COND(&ERROR = '*Error   ') THEN(GOTO ABNOR)                     /*MD054955*/
             ENDDO                                                                      /*MD054955*/
                                                                                        /*MD054955*/
/* CGRASCTD */                                                                          /*MD054955*/
             IF         COND(&FILE *EQ 'CGRASCTD') THEN(DO)                             /*MD054955*/
             CHGVAR     VAR(&LIBMOV) VALUE('#.' *TCAT %SUBSTRING(&OLDLIB 1 2) +
