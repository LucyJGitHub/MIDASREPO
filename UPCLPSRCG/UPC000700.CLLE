             GOTO       CMDLBL(END)

ABNOR:
             CHGJOB     SWS(XXXXXX11)
             DMPCLPGM

END:
             IF         COND(&LIBMOV *NE '          ') THEN(DO)                         /*MD054955*/
             RMVLIBLE   LIB(&LIBMOV)                                                    /*MD054955*/
             MONMSG     MSGID(CPF0000)                                                  /*MD054955*/
             ENDDO                                                                      /*MD054955*/
             ENDPGM
