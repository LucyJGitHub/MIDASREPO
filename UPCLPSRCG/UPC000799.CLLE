             IF         COND(&FILE *EQ 'T_WIPCF') THEN(DO)
/**********     RESETMAXID FILE(T_WIPCF)                                             */ /*MD029086*/
                RESETMAXID FILE(T_WIPCF) RSTOPT(10000)                                  /*MD029086*/
