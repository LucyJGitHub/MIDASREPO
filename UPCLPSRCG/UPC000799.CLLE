             IF         COND(&FILE *EQ 'T_GRMENUH') THEN(DO)                            /*AR859322*/
                RESETMAXID FILE(T_GRMENUH)                                              /*AR859322*/
             ENDDO                                                                      /*AR859322*/
                                                                                        /*AR859322*/
