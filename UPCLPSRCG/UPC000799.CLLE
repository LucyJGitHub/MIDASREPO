             ENDDO
 
/* GPDFFLDPD */
             IF         COND(&FILE *EQ 'GPDFFLDPD') THEN(DO)
                DLTOVR     FILE(GPDFFLDPD)
             ENDDO
 
/* FCREPTPD */
             IF         COND(&FILE *EQ 'FCREPTPD') THEN(DO)
/* Update trailer FCRPDSPA. */
                CALL       PGM(UP0828)
                IF         COND(%SWITCH(XXXXXX11)) THEN(DO)
                   RTNSUBR    RTNVAL(1)
                ENDDO
             ENDDO
 
/* APNDCONPD */
             IF         COND(&FILE *EQ 'APNDCONPD') THEN(DO)
/* Update file APDFFLDPD. */
                CALL       PGM(APCCRTNDFL)
             ENDDO
 
