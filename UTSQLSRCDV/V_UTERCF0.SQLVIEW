  CREATE VIEW                                                                   :
  **********/**********                                                         :
  AS                                                                            :
  SELECT DMMIDR                                                                 :
  FROM                                                                          :
  (SELECT * FROM UTERCDPD                                                       :
  ORDER BY DMMIDR) UTERCDPD                                                     :
  WHERE                                                                         :
  DMMIDR <> ' '                                                                 :
