     C           @CN,01    IFEQ 'D'
     C                     MOVEACTX1,2    ##CTX1
     C                     END
     C           @CN,01    IFEQ 'A'
     C                     MOVEACTX1,3    ##CTX1
     C                     END
     C           @CN,01    IFEQ 'I'
     C                     MOVEACTX1,4    ##CTX1
     C                     END
      *
     C                     MOVEL@CN,02    #DATND           Attributes Norm
      * convert from attributes - Window Layout File  *
     C* MOVE THE VALUE FROM THE FIELD TO THE DATA STRUCTURE
     C                     MOVE DNFDAT    UFATR   3
     C* CONVERT THE FIELD TYPE
     C                     TESTB'567'     UFLD1          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS NUMERIC
     C           *IN20     IFEQ '1'
     C                     MOVE 'NU'      #DIOFT
     C                     ELSE
     C                     TESTB'2'       UFLD2          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS UPPER CASE
     C           *IN20     IFEQ '1'
     C                     MOVE 'AU'      #DIOFT
     C                     ELSE
     C                     MOVE 'AL'      #DIOFT
     C                     END
     C                     END
     C* CONVERT THE ATTRIBUTES
     C                     MOVE 'X'       #DATND
     C                     TESTB'4'       UFLD3          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS BLINK
     C           *IN20     IFEQ '1'
     C                     MOVE 'X'       #DATBL
     C                     END
     C                     TESTB'5'       UFLD3          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS UNDERSCORE
     C           *IN20     IFEQ '1'
     C                     MOVE 'X'       #DATUN
     C                     END
     C                     TESTB'6'       UFLD3          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS HIGH INTENSITY
     C           *IN20     IFEQ '1'
     C                     MOVE 'X'       #DATHI
     C                     END
     C                     TESTB'7'       UFLD3          20
     C* INDICATOR 20 IS SET ON IF THE FIELD IS REVERSE IMAGE
     C           *IN20     IFEQ '1'
     C                     MOVE 'X'       #DATRI
     C                     END
      * Set up data stream fields - Window Layout File  *
     C* MOVE THE DATA STREAM FROM THE FILE TO THE FUNCTION FIELDS ON THE SCREEN
     C                     MOVELDNDSFL    #DDSN1
     C                     MOVE DNDSFL    #DDSN2
     C                     END
      *
     C   99                GOTO BEEXIT
