     C*****************************************************************
     C* #SRINT  -  INITIAL PROCESS                                    *
     C*****************************************************************
     C           #SRINT    BEGSR
     C*
     C* DEFINE CONSTANT FIELDS
     C                     Z-ADD15        C@PAG   20       LINE NUMBER
     C           C@PAG     SUB  1         C@PAG1  20         " - 1
     C*
     C* SETUP LOCAL DATA AREA
     C                     MOVEL*BLANK    DBFILE
     C                     MOVEL*BLANK    DBKEY
     C                     MOVELWSPGM     DBPGM
     C                     MOVEL*BLANK    DBASE
     C*
     C* GET RUN DATE
     C           *NAMVAR   DEFN           RUNDAT
     C                     IN   RUNDAT
     C                     MOVELRUNA      WSDMY
     C*
     C* SET SCREEN TO INITIAL AMEND MODE
     C                     MOVELCMD,1     WSCMD
     C                     SETON                     70
     C*
     C           #SINTE    ENDSR
     C*
     C/SPACE 3
     C*****************************************************************
     C* #SRCLR  -  CLEAR SUBFILE FOR NEW PAGE CREATION                *
     C*****************************************************************
     C           #SRCLR    BEGSR
     C*
     C* CLEAR SUBFILE
     C                     SETON                     40
     C                     WRITEXX1408C1
     C                     SETOF                     40
     C                     Z-ADD*ZERO     RRNMAX  40       MAX REC NO
     C*
     C                     MOVEAWSIND1    NUM
     C                     DO   6         I       20
     C                     TESTB'0123'    NUM,I          89*ON, NUMERIC
     C  N89                MOVEL'0'       NUM,I
     C                     END
     C*
     C                     ENDSR
     C*
     C/SPACE 3
     C*****************************************************************
     C* #SRFWD  -  ROLL UP TO FORWARD PAGE                            *
     C*****************************************************************
     C           #SRFWD    BEGSR
     C*
     C* CREATE SCREEN PAGE BY EXIST RECORD
     C                     SETOF                     202124
     C                     SETOF                     45
     C                     MOVEL*BLANK    WSLINE           SFL LINE
     C                     MOVELWSLINE    WSSAVE           LINE SAVE
     C                     MOVEL*BLANK    ID
     C                     Z-ADDRRNMAX    RRN     40
     C                     MOVELWKCUST    IOCUST
     C*PAGE OF AMEND MODE
     C  N30N14   IOCUST    SETLLXXCUSTP0             80
     C  N30 14   IOCUST    SETGTXXCUSTP0             80
     C*
     C  N30N80             DO   C@PAG     I
     C                     READ XXCUSTP0                 80
     C  N80      RECI      IFEQ 'D'
     C                     MOVE IOCUST    WCUST   6
     C           WCUST     CHAINSDCUSTL0             85
     C           *IN85     IFEQ '0'
     C           BBTYLC    ANDNE'D'
     C                     MOVELIOCUST    WSCNUM
     C                     MOVELBBCRNM    WSCRNM
     C                     MOVELHCNUM2    WSHCST
     C                     MOVELWSLINE    WSSAVE           LINE SAVE
     C                     ADD  1         RRN
     C                     WRITEXX1408S1
     C                     END
     C                     ELSE
     C                     SUB  1         I
     C                     END
     C        N80          END
     C*PAGE OF INSERT MODE
     C   30                DO   C@PAG
     C                     ADD  1         RRN
     C                     WRITEXX1408S1
     C                     END
     C*SET CONTROLS FOR SUBFILE
     C                     SETOF                     4142
     C           RRN       IFGT *ZERO
     C                     SETON                     41
     C  N30N80   IOCUST    SETGTXXCUSTP0             80    CHEK,RELEASE
     C  N30N80             SETON                     42
     C           RRN       IFGT RRNMAX
     C           RRNMAX    ADD  1         WSSFRC           DSP REC NO
     C                     END
     C                     END
     C*RESET KEYS FOR NEXT PAGE CREATION
     C                     Z-ADDRRN       RRNMAX           MAX REC NO
     C                     MOVELIOCUST    WKCUST
     C*
     C                     SETOF                     70
     C*
     C                     ENDSR
     C*
     C/SPACE 3
     C*****************************************************************
     C* #SRCHK  -  VALIDATION CHECK OF LIST SCREEN                    *
     C*****************************************************************
     C           #SRCHK    BEGSR
     C*
     C* CHECK SCREEN LINES (ONLY CHANGED RECORD)
     C*
     C                     SETOF                     7180
     C           *IN80     DOWEQ'0'
     C                     READCXX1408S1                 80
     C           *IN80     IFEQ '0'
     C*
     C                     SETOF                     202124
     C                     SETOF                     45
     C*
     C* GET TYPED COUNTRY CODE FOR DUPULICATE CHECK (ONLY INSERT MODE)
     C   30                MOVELWSCNUM    ID,RRN
     C*
     C* GET DISPLAY POSITION IF FIRST ERROR RECORD
     C  N79                Z-ADDRRN       WSSFRC           DSP REC NO
     C*
     C* CHECK ACTION CODE
     C           WSACTN    IFNE *BLANK
     C           WSACTN    ANDNE'D'
     C  N79                MOVELMSG,2     WSMSG
     C                     SETON                     204579
     C                     END
     C*
     C* CHECK ACCOUNT CODE
     C           WSCNUM    IFEQ *BLANK
     C           WSCNUM    OREQ *ALL'0'
     C*MANDATORY CHECK
     C           WSHCST    IFNE *BLANK
     C  N79                MOVELMSG,3     WSMSG
     C                     SETON                     214579
     C                     END
     C                     ELSE
     C*NUMERIC CHECK
     C******************** TESTN          WSCNUM     89     ON, NUMERIC EML114
     C***89                MOVE WSCNUM    WK01CR  1                     EML114
     C***89                TESTB'0123'    WK01CR         89 ON, NUMERIC EML114
     C**N89N79             MOVELMSG,4     WSMSG                         EML114
     C**N89                SETON                     214579             EML114
     C*CUSTOMER CHECK
     C           *IN30     IFEQ '1'
     C           WSCNUM    CHAINSDCUSTL0             89    ON,NOT THERE
     C  N89      BBTYLC    COMP 'D'                      89ON,IS DELETED
     C           *IN89     IFEQ '1'
     C  N79                MOVELMSG,10    WSMSG
     C                     SETON                     214579
     C                     END
     C                     END
     C*FILE CHECK
     C           *IN21     IFEQ '0'
     C                     MOVELWSCNUM    IOCUST
     C           IOCUST    CHAINXXCUSTP0             89    ON,NOT THERE
     C  N89      RECI      COMP 'D'                  8989  ON,IS DELETED
     C           *IN89     IFEQ '1'
     C     N30N79          MOVELMSG,5     WSMSG
     C     N30             SETON                     214579
     C                     ELSE
     C*DUPULICATE CHECK (ONLY INSERT MODE)
     C      30N79          MOVELMSG,6     WSMSG
     C      30             SETON                     214579
     C                     END
     C                     END
     C                     END
     C*MANDATORY CHECK
     C           WSHCST    IFEQ *BLANK
     C  N79                MOVELMSG,3     WSMSG
     C                     SETON                     244579
     C                     END
     C*
     C*SET FLAG OF FILE UPDATION IF SPECIFIED FIELDS
     C  N45      WSLINE    IFNE WSSAVE
     C                     SETON                     4571
     C                     END
     C*
     C* UPDATE SCREEN
     C                     UPDATXX1408S1
     C*
     C                     END
     C                     END
     C*
     C* CHECK DUPULICATE COUNTRY CODE BY ARRAY (ONLY INSERT MODE)
     C   30      *IN79     IFEQ '0'
     C*
     C                     DO   C@PAG1    I
     C           ID,I      IFNE *BLANK
     C           I         ADD  1         RRN
     C           ID,I      LOKUPID,RRN                   89ON,DUPLICATE
     C      89   RRN       CHAINXX1408S1             80
     C      89             SETON                     214579
     C      89             UPDATXX1408S1
     C                     END
     C                     END
     C*
     C      79             MOVELMSG,7     WSMSG
     C                     END
     C*
     C* PREPARE SCREEN IF SPECIFIED 'POSITION TO DISPLAY'
     C* OR RESET FLAG OF FILE UPDATION IF SCREEN IS ERROR
     C           *IN79     IFEQ '0'
     C   35N71             SETON                     70
     C                     ELSE
     C                     SETOF                     71
     C                     END
     C*
     C                     ENDSR
     C*
     C/SPACE 3
     C*****************************************************************
     C* #SRUPD  -  FILE UPDATION PROCESS                              *
     C*****************************************************************
     C           #SRUPD    BEGSR
     C*
     C* UPDATE FILE (ONLY CHANGED RECORD)
     C*
     C                     SETOF                     80
     C           *IN80     DOWEQ'0'
     C                     READCXX1408S1                 80
     C           *IN80     IFEQ '0'
     C*
     C* READ UPDATE FILE (ALL MODE)
     C                     MOVELWSCNUM    IOCUST
     C           IOCUST    CHAINXXCUSTP0             89
     C*
     C* DELETE MODE
     C           WSACTN    IFEQ 'D'
     C                     MOVEL'*'       RECI
     C                     MOVEL'D'       CHTP
     C                     Z-ADDRUND      LCD
     C                     ELSE
     C*
     C* INSERT OR AMEND MODE
     C                     MOVEL'D'       RECI
     C                     MOVELWSCNUM    IOCUST
     C                     MOVELWSHCST    HCNUM2
     C   89                MOVEL'I'       CHTP
     C  N89                MOVEL'A'       CHTP
     C                     Z-ADDRUND      LCD
     C                     END
     C*
     C* WRITE OR UPDATE RECORD
     C   89                WRITEXXCUSTP0
     C  N89                UPDATXXCUSTP0
     C*
     C                     END
     C                     END
     C*
     C* REFRESH SCREEN
     C                     SETON                     70
     C*
     C                     ENDSR
     C*
     C/SPACE 3
