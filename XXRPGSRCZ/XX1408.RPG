     C*
     C**  INITIAL PROCESS
     C*
     C                     EXSR #SRINT                     *INIT
     C*
     C**  MAIN SCREEN PROCESS
     C*
     C           *INKC     DOWEQ'0'
     C*ROLL UP TO FORWARD PAGE IF PRESS ROLLUP KEY
     C   70                EXSR #SRCLR
     C   14
     COR 70                EXSR #SRFWD
     C*DISPLAY
     C  N41                MOVELMSG,1     WSMSG
     C                     TIME           WSTIME
     C                     WRITEXX1408F1                   *GUIDANCE
     C                     EXFMTXX1408C1                   *LIST
     C           RRNTOP    IFGT *ZERO
     C                     Z-ADDRRNTOP    WSSFRC           DSP REC NO
     C                     END
     C                     SETOF                     36
     C                     SETOF                     79
     C                     MOVEL*BLANK    WSMSG
     C*CHECK SCREEN AND UPDATE FILE IF PRESS ENTER KEY
     C           *IN16     IFEQ '0'
     C  N41 35             SETON                     70
     C   41                EXSR #SRCHK
     C   41 71             EXSR #SRUPD
     C                     END
     C*REFRESH SCREEN IF PRESS F5 KEY
     C   KE                SETON                     36
     C*CHANGE MODE IF PRESS F9 KEY
     C   KI                SETON                     70
     C   KI      *IN30     IFEQ '0'                        AMEND MODE ?
     C                     SETON                     30     INSERT MODE
     C                     MOVELCMD,2     WSCMD
     C                     ELSE                            INSERT MODE?
     C                     SETOF                     30     AMEND MODE
     C                     MOVELCMD,1     WSCMD
     C                     END
     C*
     C                     END
     C*
     C**  END PROCESS
     C*
     C           @END      TAG
     C*
     C                     SETON                     LR
